   <text>
      <body>
         <div xml:id="workid69181" decls="#biblid69181">
            <head xml:id="idm140381326895744">rids</head>
            <lg xml:id="idm140381326895328">
               <l xml:id="idm140381326895072">viben skriger </l>
               <l xml:id="idm140381326894656">en cirkel sort </l>
               <l xml:id="idm140381326894240">skriger en kurve hvid </l>
               <l xml:id="idm140381326893824">i luften -</l>
               <l xml:id="idm140381326893408">saltengen drejer sig </l>
               <l xml:id="idm140381326892992">grøn-graa vaad </l>
               <l xml:id="idm140381326892560">med et lukket æg </l>
               <l xml:id="idm140381326892128">i midten -</l>
            </lg>
            <lg xml:id="idm140381326891584">
               <l xml:id="idm140381326891328">havet glider rødmende</l>
               <l xml:id="idm140381326890880">ind i solens ild,</l>
               <l xml:id="idm140381326890464">aften aander salt</l>
               <l xml:id="idm140381326890048">mod morgens vind -</l>
               <l xml:id="idm140381326889632">en maage forsvinder i sløret sus,</l>
               <l xml:id="idm140381326889168">fiskeren vaagner til dag -</l>
            </lg>
         </div>
      </body>
