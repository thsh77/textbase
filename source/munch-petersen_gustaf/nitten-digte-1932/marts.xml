   <text>
      <body>
         <div xml:id="workid70078" decls="#biblid70078">
            <head xml:id="idm140381326777504">marts</head>
            <lg xml:id="idm140381326777088">
               <l xml:id="idm140381326776832">sneen ligger</l>
               <l xml:id="idm140381326776416">fast og hvid -</l>
               <l xml:id="idm140381326776000">havet straaler</l>
               <l xml:id="idm140381326775584">sommerblaat -</l>
               <l xml:id="idm140381326775168">flammende sol</l>
               <l xml:id="idm140381326774752">og isklar luft</l>
               <l xml:id="idm140381326774336">brænder i trætte øjne -</l>
               <l xml:id="idm140381326773888">havfugle yngler,</l>
               <l xml:id="idm140381326773472">hæse stære skriger -</l>
               <l xml:id="idm140381326773024">natten kommer</l>
               <l xml:id="idm140381326772608">sort og dyb -</l>
               <l xml:id="idm140381326772192">baade følger</l>
               <l xml:id="idm140381326771760">stimende sild -</l>
               <l xml:id="idm140381326771344">over landet i vest</l>
               <l xml:id="idm140381326770928">gnistrer en stjærne grønt -</l>
            </lg>
         </div>
      </body>
