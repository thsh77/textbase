   <text>
      <body>
         <div xml:id="workid69294" decls="#biblid69294">
            <head xml:id="idm140381326880784">skæret</head>
            <lg xml:id="idm140381326880368">
               <l xml:id="idm140381326880112">sorthvide maager</l>
               <l xml:id="idm140381326879696">med gule næb mod sommerbrisen</l>
               <l xml:id="idm140381326879232">paa skærets ryg -</l>
               <l xml:id="idm140381326878800">en sæl drejer et sort hoved</l>
               <l xml:id="idm140381326878336">frem og tilbage kiggende</l>
               <l xml:id="idm140381326877904">langs havspejlet -</l>
               <l xml:id="idm140381326877488">skærets eneste asters brænder violet</l>
               <l xml:id="idm140381326877008">i solen -</l>
               <l xml:id="idm140381326876592">fluerne danser over</l>
               <l xml:id="idm140381326876176">maagernes mødding -</l>
               <l xml:id="idm140381326875728">sælen forsvinder -</l>
               <l xml:id="idm140381326875296">maagerne letter -</l>
               <l xml:id="idm140381326874880">strandastersen sidder fast i sin revne -</l>
               <l xml:id="idm140381326874432">et rødt sejl dukker op -</l>
            </lg>
         </div>
      </body>
