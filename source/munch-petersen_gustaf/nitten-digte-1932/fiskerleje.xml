   <text>
      <body>
         <div xml:id="workid69244" decls="#biblid69244">
            <head xml:id="idm140381326887520">fiskerleje</head>
            <lg xml:id="idm140381326887104">
               <l xml:id="idm140381326886848">oktobers horisont</l>
               <l xml:id="idm140381326886432">er højere end kirkens spir -</l>
               <l xml:id="idm140381326885968">paa dens takkede kant</l>
               <l xml:id="idm140381326885552">vandrer smaa dampere</l>
               <l xml:id="idm140381326885136">i række -</l>
               <l xml:id="idm140381326884720">paa kysten hamrer havet sort</l>
               <l xml:id="idm140381326884288">trommende -</l>
               <l xml:id="idm140381326883872">og store vejrbidte mænd</l>
               <l xml:id="idm140381326883424">kløver pindebrænde</l>
               <l xml:id="idm140381326882976">med smaa økser -</l>
            </lg>
            <pb n="115" xml:id="idm140381326882416" rend="missing"/>
         </div>
      </body>
