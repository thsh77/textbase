   <text>
      <body>
         <div xml:id="workid69714" decls="#biblid69714">
            <head xml:id="idm140381326825328">se!</head>
            <lg xml:id="idm140381326824944">
               <l xml:id="idm140381326824688">se solen, solen,</l>
               <l xml:id="idm140381326824272">føl luften i det levende bryst,</l>
               <l xml:id="idm140381326823808">græsset under foden i glæde, glæde -,</l>
               <l xml:id="idm140381326823328">endnu lever du jo -!</l>
               <l xml:id="idm140381326822912">o, se, solen staar op,</l>
               <l xml:id="idm140381326822496">se, dit barn sover,</l>
               <l xml:id="idm140381326822080">- se -!</l>
               <l xml:id="idm140381326821664">endnu lever du jo,</l>
               <l xml:id="idm140381326821248">din kvinde glæder sig i dit hus -</l>
               <l xml:id="idm140381326820784">- fyld dine øjne med det varme, herlige lys -, </l>
               <l xml:id="idm140381326820288">levende broder!</l>
            </lg>
         </div>
      </body>
   </text>
