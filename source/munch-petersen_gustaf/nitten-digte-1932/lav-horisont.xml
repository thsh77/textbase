   <text>
      <body>
         <div xml:id="workid69408" decls="#biblid69408">
            <head xml:id="idm140381326865712">lav horisont</head>
            <lg xml:id="idm140381326865296">
               <l xml:id="idm140381326865040">regngraa stormvind</l>
               <l xml:id="idm140381326864624">trykker vintersæds blaagrønne spirer</l>
               <l xml:id="idm140381326864144">mod sorte knolde -</l>
               <pb n="116" xml:id="idm140381326863728" rend="missing"/>
               <l xml:id="idm140381326863184">markerne bølger tungt,</l>
               <l xml:id="idm140381326862736">dunkel-grønne haar følger</l>
               <l xml:id="idm140381326862288">i nikkende dans stormens</l>
               <l xml:id="idm140381326861856">isnende strøg over krybende muld -</l>
               <l xml:id="idm140381326861392">himlen kaster sig sort udad</l>
               <l xml:id="idm140381326860960">mod havet -</l>
               <l xml:id="idm140381326860544">grønne spirer nikker i takt</l>
               <l xml:id="idm140381326860080">flimrende under stormens slør -</l>
            </lg>
         </div>
      </body>
