   <text>
      <body>
         <div xml:id="workid69866" decls="#biblid69866">
            <head xml:id="idm140381326804992">vise</head>
            <lg xml:id="idm140381326804576">
               <l xml:id="idm140381326804320">mørke ansigter rundt omkring -</l>
               <l xml:id="idm140381326803856">de klare glas paa bordet -</l>
               <l xml:id="idm140381326803424">den hvide brænding</l>
               <l xml:id="idm140381326802992">den hvide sne</l>
               <l xml:id="idm140381326802576">og evig storm,</l>
               <l xml:id="idm140381326802160">vort svage mod</l>
               <l xml:id="idm140381326801744">i verdens larm -</l>
               <l xml:id="idm140381326801328">ned foran glassets rand</l>
               <l xml:id="idm140381326800912">sænker sig den almægtiges skæg</l>
               <l xml:id="idm140381326800448">hans vældige ro, hans bydende blik,</l>
               <l xml:id="idm140381326799968">hans grædefærdige smil -</l>
               <l xml:id="idm140381326799520">den hvide brænding</l>
               <l xml:id="idm140381326799088">den hvide sne</l>
               <l xml:id="idm140381326798672">og evig storm,</l>
               <l xml:id="idm140381326798256">vort svage mod</l>
               <l xml:id="idm140381326797840">i verdens larm -</l>
               <l xml:id="idm140381326797424">drik ud,</l>
               <l xml:id="idm140381326797008">gud hjælpe os smaa -</l>
            </lg>
         </div>
      </body>
