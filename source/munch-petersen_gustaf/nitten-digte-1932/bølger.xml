   <text>
      <body>
         <div xml:id="workid69077" decls="#biblid69077">
            <head xml:id="idm140381326909536">bølger</head>
            <lg xml:id="idm140381326909120">
               <l xml:id="idm140381326908864">- solens sendags-skraa spyd </l>
               <l xml:id="idm140381326908432">svingende stikker i havet, </l>
               <l xml:id="idm140381326908000">glashuden hæver sig dirrende </l>
               <l xml:id="idm140381326907536">og glattes ud,</l>
               <l xml:id="idm140381326907120">kysten gennem dis er grøn,</l>
               <l xml:id="idm140381326906672">blegblaarøde vandmænd stiger</l>
               <l xml:id="idm140381326906208">mod guldlys,</l>
               <l xml:id="idm140381326905792">suges mod havbund,</l>
               <l xml:id="idm140381326905376">driver bølgende mod mad og død -</l>
               <l xml:id="idm140381326904912">solpile vakler og falder,</l>
               <l xml:id="idm140381326904480">kysten vender sig bort,</l>
               <l xml:id="idm140381326904064">vindstille matblaa bølger</l>
               <l xml:id="idm140381326903616">mumler havbølgers nat -</l>
            </lg>
         </div>
      </body>
   </text>
