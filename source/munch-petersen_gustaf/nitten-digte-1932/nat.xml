   <text>
      <body>
         <div xml:id="workid69600" decls="#biblid69600">
            <head xml:id="idm140381326840400">nat</head>
            <lg xml:id="idm140381326840016">
               <l xml:id="idm140381326839760">stille regn falder</l>
               <l xml:id="idm140381326839344">gennem natten -</l>
               <l xml:id="idm140381326838928">over den anden kyst</l>
               <l xml:id="idm140381326838512">hviner stormen -</l>
               <l xml:id="idm140381326838096">her falder regnen stille -</l>
               <l xml:id="idm140381326837664">fra en graa himmel</l>
               <l xml:id="idm140381326837248">usynlig i natten</l>
               <l xml:id="idm140381326836832">falder regndraaberne</l>
               <l xml:id="idm140381326836416">paa huse og marker og baade,</l>
               <l xml:id="idm140381326835984">og paa klipperne ved havet -</l>
               <l xml:id="idm140381326835552">og gør alting vaadt</l>
               <l xml:id="idm140381326835104">i nattens stilhed -</l>
               <l xml:id="idm140381326834688">fra den anden side øen</l>
               <l xml:id="idm140381326834240">høres stormens hvinen</l>
               <l xml:id="idm140381326833792">svagt -</l>
            </lg>
         </div>
      </body>
