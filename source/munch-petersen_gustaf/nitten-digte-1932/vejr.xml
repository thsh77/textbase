   <text>
      <body>
         <div xml:id="workid69355" decls="#biblid69355">
            <head xml:id="idm140381326872720">vejr</head>
            <lg xml:id="idm140381326872304">
               <l xml:id="idm140381326872048">- østenstormen klemmer </l>
               <l xml:id="idm140381326871600">havet mod klippernes tænder </l>
               <l xml:id="idm140381326871136">lukker havnene</l>
               <l xml:id="idm140381326870720">jager fisken </l>
               <l xml:id="idm140381326870304">til havs -</l>
            </lg>
            <lg xml:id="idm140381326869760">
               <l xml:id="idm140381326869504">- skyerne blir smaa og stjernerne kommer </l>
               <l xml:id="idm140381326869056">efter tre døgn -</l>
               <l xml:id="idm140381326868624">vestbrise puffer søerne væk, </l>
               <l xml:id="idm140381326868160">gammel brænding falder </l>
               <l xml:id="idm140381326867712">tung paa skæret - </l>
               <l xml:id="idm140381326867280">fiskerne kigger -</l>
            </lg>
         </div>
      </body>
