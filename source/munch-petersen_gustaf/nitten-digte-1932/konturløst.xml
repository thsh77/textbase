   <text>
      <body>
         <div xml:id="workid69535" decls="#biblid69535">
            <head xml:id="idm140381326848896">konturløst</head>
            <lg xml:id="idm140381326848480">
               <l xml:id="idm140381326848224">oktober regn-storm pisker fra land,</l>
               <l xml:id="idm140381326847792">havet kryber,</l>
               <l xml:id="idm140381326847376">bygerne hvinende stryger,</l>
               <l xml:id="idm140381326846944">lave sorte, pressende havet ned -</l>
               <l xml:id="idm140381326846512">vandet synker</l>
               <l xml:id="idm140381326846096">skærene ligger blottede,</l>
               <l xml:id="idm140381326845648">et opankret skib</l>
               <pb n="117" xml:id="idm140381326845232" rend="missing"/>
               <l xml:id="idm140381326844736">staar taarnhøjt, opslugt i dis, </l>
               <l xml:id="idm140381326844272">en kone paa dækket </l>
               <l xml:id="idm140381326843824">med kikkert ser intet - </l>
               <l xml:id="idm140381326843392">over landet en tidlig maane </l>
               <l xml:id="idm140381326842960">suser gennem rygende skyer - </l>
               <l xml:id="idm140381326842528">en lille baad flygter </l>
               <l xml:id="idm140381326842112">væltende mod havn -</l>
            </lg>
         </div>
      </body>
