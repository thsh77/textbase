   <text>
      <body>
         <div xml:id="workid69135" decls="#biblid69135">
            <head xml:id="idm140381326902016">hvidt</head>
            <lg xml:id="idm140381326901600">
               <l xml:id="idm140381326901344">en hvid bugnende snesky</l>
               <l xml:id="idm140381326900928">en hvid skraat spejlende maane</l>
               <l xml:id="idm140381326900496">et barns  skrig</l>
               <pb n="114" xml:id="idm140381326900080" rend="missing"/>
               <l xml:id="idm140381326899536">en hvidmalet baad paa bedding </l>
               <l xml:id="idm140381326899104">sorte bølgers merke skvulp </l>
               <l xml:id="idm140381326898640">og skridt af træsko </l>
               <l xml:id="idm140381326898192">paa en sildeskællet kaj </l>
               <l xml:id="idm140381326897744">tegner fosforsyngende </l>
               <l xml:id="idm140381326897328">oktobernattens bidende kontur -</l>
            </lg>
         </div>
      </body>
