   <text>
      <body>
         <div xml:id="workid69663" decls="#biblid69663">
            <head xml:id="idm140381326832224">pynten</head>
            <lg xml:id="idm140381326831808">
               <l xml:id="idm140381326831552">selv naar det er stille, </l>
               <l xml:id="idm140381326831120">naar blot en enkelt graa sky </l>
               <l xml:id="idm140381326830688">haenger tung ved solnedgang, </l>
               <l xml:id="idm140381326830256">rejser bølgen sig højt i vejret </l>
               <l xml:id="idm140381326829792">paa skæret ud for pynten - </l>
               <l xml:id="idm140381326829328">mod pyntens sorte stenknold </l>
               <l xml:id="idm140381326828896">falder bølgen ned igen </l>
               <l xml:id="idm140381326828448">med sne-hvidt brus - </l>
               <l xml:id="idm140381326828032">under tungt hængende himmel </l>
               <l xml:id="idm140381326827568">med en gul stribe i vest -</l>
            </lg>
            <pb n="118" xml:id="idm140381326827008" rend="missing"/>
         </div>
      </body>
