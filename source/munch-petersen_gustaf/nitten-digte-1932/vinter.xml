   <text>
      <body>
         <div xml:id="workid69940" decls="#biblid69940">
            <head xml:id="idm140381326795408">vinter</head>
            <lg xml:id="idm140381326794992">
               <l xml:id="idm140381326794736">stormene buldrer -</l>
               <l xml:id="idm140381326794320">vinteren fører sin krig -</l>
               <l xml:id="idm140381326793872">havet vandrer</l>
               <l xml:id="idm140381326793456">langs kysterne -</l>
               <l xml:id="idm140381326793040">himlen følger det</l>
               <l xml:id="idm140381326792608">højt over landet -</l>
               <l xml:id="idm140381326792176">smaa huse trykker sig</l>
               <l xml:id="idm140381326791760">i angst mod jorden -</l>
               <l xml:id="idm140381326791344">maanens øje af is</l>
               <l xml:id="idm140381326790912">stirrer gennem nattens mur</l>
               <l xml:id="idm140381326790480">paa mænd, der henter kul -</l>
            </lg>
         </div>
      </body>
   </text>
