   <text>
      <body>
         <div xml:id="workid72691" decls="#biblid72691">
            <head xml:id="idm140462578325760">afsked</head>
            <lg xml:id="idm140462578325344">
               <l xml:id="idm140462578325088">en makrelstime er en regn</l>
               <l xml:id="idm140462578324656">af undervejsdræbende</l>
               <l xml:id="idm140462578324208">strygende sortrygge -</l>
               <l xml:id="idm140462578323792">havet har ingen lyd -</l>
               <l xml:id="idm140462578323376">mit</l>
               <l xml:id="idm140462578322944">hjærtes læber er to</l>
               <l xml:id="idm140462578322496">makrelrygge</l>
               <l xml:id="idm140462578322080">du er et offer -</l>
               <l xml:id="idm140462578321664">havet hvisker retningen</l>
               <l xml:id="idm140462578321248">med stor fart -</l>
               <l xml:id="idm140462578320832">tramp tramp tramp</l>
               <l xml:id="idm140462578320416">i dit hjærte -</l>
               <l xml:id="idm140462578319984">jeg ved det -</l>
               <l xml:id="idm140462578319568">du faar mit had for det -</l>
            </lg>
         </div>
      </body>
