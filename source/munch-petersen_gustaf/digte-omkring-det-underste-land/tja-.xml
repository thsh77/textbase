   <text>
      <body>
         <div xml:id="workid72067" decls="#biblid72067">
            <head xml:id="idm140462578404080">tja-</head>
            <lg xml:id="idm140462578403664">
               <l xml:id="idm140462578403408">en dag fandt vi os</l>
               <l xml:id="idm140462578402992">en graadighed,</l>
               <l xml:id="idm140462578402576">et begær efter ny kraft,</l>
               <l xml:id="idm140462578402128">som intet af det, vi kendte,</l>
               <l xml:id="idm140462578401696">kunde mætte -</l>
            </lg>
            <lg xml:id="idm140462578401136">
               <l xml:id="idm140462578400880">da blev for nogle af os</l>
               <l xml:id="idm140462578400464">arbejdet meningsløst</l>
               <pb n="137" xml:id="idm140462578400016" rend="missing"/>
               <l xml:id="idm140462578399520">og skønheden dum -</l>
               <l xml:id="idm140462578399088">meget slog vi i stykker -</l>
               <l xml:id="idm140462578398656">mere for at høre klirren</l>
               <l xml:id="idm140462578398208">og for at mærke</l>
               <l xml:id="idm140462578397776">den sviende smærte fra smaa saar -</l>
            </lg>
         </div>
      </body>
