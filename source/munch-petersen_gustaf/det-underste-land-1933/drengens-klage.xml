   <text>
      <body>
         <div xml:id="workid68163" decls="#biblid68163">
            <head xml:id="idm140462578914080">drengens klage</head>
            <lg xml:id="idm140462578913664">
               <l xml:id="idm140462578913408">hvor kan jeg gemme mig</l>
               <l xml:id="idm140462578912992">hvor kan jeg gemme mig,</l>
               <l xml:id="idm140462578912576">o i nat vil de alle komme</l>
               <l xml:id="idm140462578912144">vil de alle komme fra hele verden -</l>
               <l xml:id="idm140462578911712">o findes der ikke</l>
               <l xml:id="idm140462578911296">et eneste skjulested</l>
               <l xml:id="idm140462578910880">bare et eneste skjulested</l>
               <l xml:id="idm140462578910448">til mig</l>
               <l xml:id="idm140462578910032">i nat -</l>
               <l xml:id="idm140462578909616">jeg er saa bange</l>
               <l xml:id="idm140462578909200">jeg dør af angst,</l>
               <l xml:id="idm140462578908768">naar ingen synger i hele verden</l>
               <l xml:id="idm140462578908336">om natten -</l>
            </lg>
            <lg xml:id="idm140462578907792">
               <l xml:id="idm140462578907536">o hvor kan jeg gemme mig</l>
               <l xml:id="idm140462578907104">gemme mig bare i nat -</l>
               <l xml:id="idm140462578906688">alle de store sange</l>
               <l xml:id="idm140462578906272">er døde hjemme</l>
               <l xml:id="idm140462578905840">hjemme er ikke flere store sange</l>
               <l xml:id="idm140462578905408">nu synger de kun smaa sange</l>
               <l xml:id="idm140462578904976">hjemme -</l>
            </lg>
            <lg xml:id="idm140462578904432">
               <l xml:id="idm140462578904176">o jeg dør af angst,</l>
               <l xml:id="idm140462578903728">naar ingen synger i hele verden</l>
               <l xml:id="idm140462578903296">om natten -</l>
            </lg>
         </div>
