   <text>
      <body>
         <div xml:id="workid71237" decls="#biblid71237">
            <head xml:id="idm140462578511024">brødre</head>
            <lg xml:id="idm140462578510608">
               <l xml:id="idm140462578510352">de vogtede geder</l>
               <l xml:id="idm140462578509936">og jeg bredte mine fødder i solen</l>
               <l xml:id="idm140462578509472">paa capo san andreae -</l>
               <l xml:id="idm140462578509056">sortøjet, syvaarig og smilende</l>
               <l xml:id="idm140462578508592">kom den ældste og tiggede cigaretter -</l>
               <l xml:id="idm140462578508112">da han gik,</l>
               <l xml:id="idm140462578507696">slog han tre saltomortaler</l>
               <l xml:id="idm140462578507264">og lo -</l>
               <l xml:id="idm140462578506848">brunkindet,  taarestribet</l>
               <l xml:id="idm140462578506416">kom den lille</l>
               <l xml:id="idm140462578506000">og lagde sukkende fem gule blomster</l>
               <l xml:id="idm140462578505568">ved mine fødder</l>
               <l xml:id="idm140462578505136">paa capo san andreae</l>
            </lg>
            <pb n="131" xml:id="idm140462578504592" rend="missing"/>
         </div>
