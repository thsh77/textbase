   <text>
      <body>
         <div xml:id="workid68980" decls="#biblid68980">
            <head xml:id="idm140462578809488">ivigtut</head>
            <lg xml:id="idm140462578809072">
               <l xml:id="idm140462578808816">de hundrede mand</l>
               <l xml:id="idm140462578808400">graver sten</l>
               <pb n="114" xml:id="idm140462578807984" rend="missing"/>
               <l xml:id="idm140462578807488">i elleve timer -,</l>
               <l xml:id="idm140462578807072">og fylder store skibe</l>
               <l xml:id="idm140462578806656">og fremmede aktionærer,</l>
               <l xml:id="idm140462578806208">som de gir fa'en -</l>
               <l xml:id="idm140462578805792">(elleve timer -)</l>
            </lg>
            <lg xml:id="idm140462578805248">
               <l xml:id="idm140462578804992">og de hundrede mand</l>
               <l xml:id="idm140462578804576">hælder champagne</l>
               <l xml:id="idm140462578804144">i sjæle,</l>
               <l xml:id="idm140462578803728">som længslernes by</l>
               <l xml:id="idm140462578803296">burde frygte,</l>
               <l xml:id="idm140462578802880">og ikke kunde fylde</l>
               <l xml:id="idm140462578802464">for to aar siden -</l>
            </lg>
            <lg xml:id="idm140462578801920">
               <l xml:id="idm140462578801664">og de hundrede mand</l>
               <l xml:id="idm140462578801248">rejser hjem</l>
               <l xml:id="idm140462578800832">(efter to eller tre aar)</l>
               <l xml:id="idm140462578800400">med længsler</l>
               <l xml:id="idm140462578799968">tunge som stenbunker</l>
               <l xml:id="idm140462578799552">og uberegnelige som isfjelde -</l>
            </lg>
            <lg xml:id="idm140462578798992">
               <l xml:id="idm140462578798736">og de hundrede mand</l>
               <l xml:id="idm140462578798320">sejler frem og tilbage</l>
               <l xml:id="idm140462578797904">(saa længe de kan)</l>
               <l xml:id="idm140462578797472">med deres mystiske sjæle,</l>
               <l xml:id="idm140462578797024">som de ikke kan</l>
               <l xml:id="idm140462578796608">gi fa'en -</l>
            </lg>
         </div>
      </body>
