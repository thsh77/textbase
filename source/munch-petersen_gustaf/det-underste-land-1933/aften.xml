   <text>
      <body>
         <div xml:id="workid67639" decls="#biblid67639">
            <head xml:id="idm140462578980576">aften</head>
            <lg xml:id="idm140462578980160">
               <l xml:id="idm140462578979904">jeg klagede -:</l>
               <l xml:id="idm140462578979488">større end alt var min elskede -,</l>
               <l xml:id="idm140462578979024">større end alt -</l>
            </lg>
            <lg xml:id="idm140462578978464">
               <l xml:id="idm140462578978208">og jeg gik -</l>
            </lg>
            <lg xml:id="idm140462578977664">
               <l xml:id="idm140462578977408">en aften ved den sovende flod</l>
               <l xml:id="idm140462578976976">huskede jeg,</l>
               <l xml:id="idm140462578976560">at sangen om den store kærlighed</l>
               <l xml:id="idm140462578976096">spilles vidunderligst med to pinde</l>
               <l xml:id="idm140462578975664">mod et stykke træ -</l>
            </lg>
         </div>
      </body>
