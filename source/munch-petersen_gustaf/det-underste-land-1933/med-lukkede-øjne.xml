   <text>
      <body>
         <div xml:id="workid67202" decls="#biblid67202">
            <head xml:id="idm140462579036736">med lukkede øjne</head>
            <lg xml:id="idm140462579036304">
               <l xml:id="idm140462579036048">det er som om</l>
               <l xml:id="idm140462579035632">ingen strøm kan drukne mig</l>
               <l xml:id="idm140462579035184">ingen sorg kvæle mig</l>
               <l xml:id="idm140462579034736">helt -</l>
               <l xml:id="idm140462579034320">det er som om</l>
               <l xml:id="idm140462579033904">kærlighed kommer til mig</l>
               <l xml:id="idm140462579033456">over alle have,</l>
               <l xml:id="idm140462579033040">fordi en blød streng altid svinger</l>
               <l xml:id="idm140462579032576">i mig -</l>
            </lg>
         </div>
      </body>
   </text>
