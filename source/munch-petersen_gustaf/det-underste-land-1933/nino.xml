   <text>
      <body>
         <div xml:id="workid71397" decls="#biblid71397">
            <head xml:id="idm140462578490480">nino</head>
            <lg xml:id="idm140462578490064">
               <l xml:id="idm140462578489808">han blev fundet under citrontræerne -</l>
               <l xml:id="idm140462578489328">han blev tvangsgift nittenaarig -</l>
               <l xml:id="idm140462578488896">han mistede sin manddom ved marne -</l>
               <l xml:id="idm140462578488464">og han føder elleve tiggere -</l>
            </lg>
            <lg xml:id="idm140462578487872">
               <l xml:id="idm140462578487616">inderligheden</l>
               <l xml:id="idm140462578487200">i flygtende elefanters løb -:</l>
               <l xml:id="idm140462578486736">din gang -</l>
            </lg>
            <pb n="132" xml:id="idm140462578486192" rend="missing"/>
            <lg xml:id="idm140462578485648">
               <l xml:id="idm140462578485392">tavse, hungerfri</l>
               <l xml:id="idm140462578484976">dine øjne sover under faraopanden -</l>
               <l xml:id="idm140462578484496">og med elskerhænder serverer du kaffe -</l>
            </lg>
            <lg xml:id="idm140462578483888">
               <l xml:id="idm140462578483632">i cafe nuovo</l>
               <l xml:id="idm140462578483216">sidder mænd og kvinder -</l>
               <l xml:id="idm140462578482768">og mændene</l>
               <l xml:id="idm140462578482352">slaar</l>
               <l xml:id="idm140462578481936">pengene i bordet -,</l>
               <l xml:id="idm140462578481520">og kvinderne sidder tavse -</l>
               <l xml:id="idm140462578481088">og nino bukker -</l>
            </lg>
            <lg xml:id="idm140462578480544">
               <l xml:id="idm140462578480288">og en aften vendte sig amerikas millionskøge</l>
               <l xml:id="idm140462578479792">mod sin nye millionær</l>
               <l xml:id="idm140462578479344">og skreg som en kælling -:</l>
               <l xml:id="idm140462578478896">"gaa -!"</l>
               <l xml:id="idm140462578478480">og hjælpeløst bed hun</l>
               <l xml:id="idm140462578478032">i hummerrøde negle</l>
               <l xml:id="idm140462578477600">som en dreng -</l>
            </lg>
            <lg xml:id="idm140462578477056">
               <l xml:id="idm140462578476800">og ninos øjne</l>
               <l xml:id="idm140462578476368">sov tretusenaarig faraosøvn -,</l>
               <l xml:id="idm140462578475904">og tjeneren nino bukkede -</l>
            </lg>
         </div>
      </body>
