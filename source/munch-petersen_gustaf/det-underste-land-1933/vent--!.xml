   <text>
      <body>
         <div xml:id="workid67942" decls="#biblid67942">
            <head xml:id="idm140462578942016">vent -!</head>
            <lg xml:id="idm140462578941600">
               <l xml:id="idm140462578941344">hun venter -</l>
               <l xml:id="idm140462578940928">intet er for stort -</l>
               <l xml:id="idm140462578940512">foroverbøjet sidder hun -</l>
               <l xml:id="idm140462578940064">panden stirrer -</l>
               <l xml:id="idm140462578939648">natten trækker lange lange</l>
               <l xml:id="idm140462578939200">nynnende fløjelsbaand gennem hendes aabne pupiller-</l>
               <l xml:id="idm140462578938688">sagte famler næsefløjene</l>
               <l xml:id="idm140462578938240">vælgende i overflodet -</l>
               <l xml:id="idm140462578937792">intet er for stort -</l>
               <l xml:id="idm140462578937376">taalmodigt venter opadvendte haandflader</l>
               <l xml:id="idm140462578936928">den første draabe -</l>
            </lg>
         </div>
      </body>
