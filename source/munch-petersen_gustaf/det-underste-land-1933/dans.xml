   <text>
      <body>
         <div xml:id="workid70464" decls="#biblid70464">
            <head xml:id="idm140462578607696">dans</head>
            <lg xml:id="idm140462578607280">
               <l xml:id="idm140462578607024">lad mine ører i fred</l>
               <l xml:id="idm140462578606576">for din smerte</l>
               <l xml:id="idm140462578606160">lad mine øjne i fred</l>
               <l xml:id="idm140462578605712">for din smerte -!</l>
            </lg>
            <lg xml:id="idm140462578605168">
               <l xml:id="idm140462578604912">jeg har en hel</l>
               <l xml:id="idm140462578604496">verdens sorger</l>
               <l xml:id="idm140462578604080">forbandelser</l>
               <l xml:id="idm140462578603664">og vildskab</l>
               <l xml:id="idm140462578603248">et sted -</l>
            </lg>
            <pb n="125" xml:id="idm140462578602704" rend="missing"/>
            <lg xml:id="idm140462578602208">
               <l xml:id="idm140462578601952">en hel forbandet verdens vilde sorger</l>
               <l xml:id="idm140462578601520">har jeg -</l>
               <l xml:id="idm140462578601104">jeg danser</l>
               <l xml:id="idm140462578600688">jeg danser -</l>
            </lg>
            <lg xml:id="idm140462578600144">
               <l xml:id="idm140462578599888">lad mig i fred -!</l>
               <l xml:id="idm140462578599472">jeg har</l>
               <l xml:id="idm140462578599056">alle slags elskov</l>
               <l xml:id="idm140462578598640">i en herlig stue</l>
               <l xml:id="idm140462578598224">med sort dør -</l>
            </lg>
            <lg xml:id="idm140462578597664">
               <l xml:id="idm140462578597408">nu</l>
               <l xml:id="idm140462578597024">er der tænder i navlestrengen -</l>
               <l xml:id="idm140462578596560">gaa væk</l>
               <l xml:id="idm140462578596144">gaa væk</l>
               <l xml:id="idm140462578595728">nu danser jeg -</l>
               <l xml:id="idm140462578595312">en fed streg tværsover -</l>
            </lg>
            <lg xml:id="idm140462578594736">
               <l xml:id="idm140462578594480">lad mig i fred</l>
               <l xml:id="idm140462578594064">jeg har et dybt saar -</l>
            </lg>
            <lg xml:id="idm140462578593520">
               <l xml:id="idm140462578593264">o, nu danser jeg</l>
               <l xml:id="idm140462578592848">alene -</l>
               <l xml:id="idm140462578592432">nu gaar det</l>
               <l xml:id="idm140462578592016">nu danser jeg</l>
               <l xml:id="idm140462578591600">alene -</l>
            </lg>
            <lg xml:id="idm140462578591056">
               <l xml:id="idm140462578590800">nu falder alle haver</l>
               <l xml:id="idm140462578590384">det, som er færdigt,</l>
               <l xml:id="idm140462578589936">ler -</l>
            </lg>
         </div>
      </body>
