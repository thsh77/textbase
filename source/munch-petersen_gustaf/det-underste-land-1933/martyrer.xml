   <text>
      <body>
         <div xml:id="workid68818" decls="#biblid68818">
            <head xml:id="idm140462578830336">martyrer</head>
            <lg xml:id="idm140462578829920">
               <l xml:id="idm140462578829664">i, som kuede jeres munde -,</l>
               <l xml:id="idm140462578829232">tvang jeres øjne udad -,</l>
               <l xml:id="idm140462578828784">ogsaa jeg</l>
               <l xml:id="idm140462578828368">elsker jer,</l>
               <l xml:id="idm140462578827952">der først vendte pisken</l>
               <l xml:id="idm140462578827504">mod egne drømme</l>
               <l xml:id="idm140462578827072">og sagde: lig!</l>
               <l xml:id="idm140462578826656">og saa rakte sværdet</l>
            </lg>
            <pb n="113" xml:id="idm140462578826080" rend="missing"/>
            <lg xml:id="idm140462578825584">
               <l xml:id="idm140462578825328">over</l>
               <l xml:id="idm140462578824912">de smaa og faldne</l>
               <l xml:id="idm140462578824496">mod de store frække -,</l>
               <l xml:id="idm140462578824048">glemmende egen graad</l>
               <l xml:id="idm140462578823632">for andres nød -</l>
               <l xml:id="idm140462578823200">jeg elsker jer -</l>
            </lg>
            <lg xml:id="idm140462578822656">
               <l xml:id="idm140462578822400">o at i maatte leve</l>
               <l xml:id="idm140462578821984">til de,</l>
               <l xml:id="idm140462578821568">som i nu gir brød</l>
               <l xml:id="idm140462578821136">kommer for at kræve</l>
               <l xml:id="idm140462578820688">ogsaa jeres drømme og jeres graad -,</l>
               <l xml:id="idm140462578820208">og i kan kaste pisken</l>
               <l xml:id="idm140462578819792">og gi alt -</l>
            </lg>
         </div>
      </body>
