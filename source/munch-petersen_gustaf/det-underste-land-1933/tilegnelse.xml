   <text>
      <body>
         <div xml:id="workid67089" decls="#biblid67089">
            <head xml:id="idm140462579053296">tilegnelse</head>
            <lg xml:id="idm140462579052880">
               <l xml:id="idm140462579052624">
                  <hi rend="italics" xml:id="idm140462579052368">paa vejen blev</hi>
               </l>
               <l xml:id="idm140462579051552">
                  <hi rend="italics" xml:id="idm140462579051296">mit hjærte alt for tungt at slæbe</hi>
               </l>
               <l xml:id="idm140462579050432">
                  <hi rend="italics" xml:id="idm140462579050176">her lagde jeg denne del</hi>
               </l>
               <l xml:id="idm140462579049360">
                  <hi rend="italics" xml:id="idm140462579049104">ved vejkanten</hi>
               </l>
               <l xml:id="idm140462579048288">
                  <hi rend="italics" xml:id="idm140462579048160">men ogsaa dette tilkommer dig</hi> -</l>
               <l xml:id="idm140462579047328">
                  <hi rend="italics" xml:id="idm140462579047072">her</hi>
               </l>
               <l xml:id="idm140462579046240">
                  <hi rend="italics" xml:id="idm140462579045984">stedet, hvor jeg hvilede sidst</hi>
               </l>
               <l xml:id="idm140462579045152">
                  <hi rend="italics" xml:id="idm140462579045024">og maatte lægge dette</hi> -</l>
               <l xml:id="idm140462579044176">
                  <hi rend="italics" xml:id="idm140462579044048">ogsaa det tilhører dig</hi> -</l>
               <l xml:id="idm140462579043200">
                  <hi rend="italics" xml:id="idm140462579042944">over dine øjne</hi>
               </l>
               <l xml:id="idm140462579042112">
                  <hi rend="italics" xml:id="idm140462579041856">sidder alle guds smil</hi>
               </l>
               <l xml:id="idm140462579041040">
                  <hi rend="italics" xml:id="idm140462579040784">roligt afventende</hi>
               </l>
            </lg>
            <l xml:id="idm140462579039840">
               <hi rend="italics" xml:id="idm140462579039712">min sikre herliggørelse</hi> -</l>
            <pb n="100" xml:id="idm140462579038864" rend="missing"/>
