   <text>
      <body>
         <div xml:id="workid68752" decls="#biblid68752">
            <head xml:id="idm140462578838992">sump</head>
            <lg xml:id="idm140462578838576">
               <l xml:id="idm140462578838320">jeg har en sjæl -</l>
               <l xml:id="idm140462578837888">i over et aar</l>
               <l xml:id="idm140462578837472">har jeg vendt og drejet den,</l>
               <l xml:id="idm140462578837040">vredet og glattet den ud -</l>
            </lg>
            <lg xml:id="idm140462578836480">
               <l xml:id="idm140462578836224">jo mere jeg skrubbede,</l>
               <l xml:id="idm140462578835808">jo renere blev den -</l>
               <l xml:id="idm140462578835392">jo mere jeg stak,</l>
               <l xml:id="idm140462578834976">jo mere sved det -</l>
               <l xml:id="idm140462578834560">og jeg bemærkede dette</l>
               <l xml:id="idm140462578834112">med uro og alvor -</l>
            </lg>
            <lg xml:id="idm140462578833568">
               <l xml:id="idm140462578833312">i mellemtiden</l>
               <l xml:id="idm140462578832896">sprøjtede mit legeme</l>
               <l xml:id="idm140462578832448">unyttig sæd</l>
               <l xml:id="idm140462578832016">over uskyldige -</l>
            </lg>
         </div>
      </body>
