   <text>
      <body>
         <div xml:id="workid71178" decls="#biblid71178">
            <head xml:id="idm140462578518784">salvatore bambara</head>
            <lg xml:id="idm140462578518368">
               <l xml:id="idm140462578518112">alle sang -</l>
            </lg>
            <lg xml:id="idm140462578517568">
               <l xml:id="idm140462578517312">salvatore bambara</l>
               <l xml:id="idm140462578516896">sad alene</l>
               <l xml:id="idm140462578516480">og saa paa sine hænder -</l>
               <l xml:id="idm140462578516032">saa løftede han øjnene</l>
               <l xml:id="idm140462578515584">smilende</l>
               <l xml:id="idm140462578515168">lyttende -</l>
               <l xml:id="idm140462578514752">o de er glade</l>
               <l xml:id="idm140462578514336">og det er mine ord, de synger,</l>
               <l xml:id="idm140462578513904">og ingen ved det -</l>
               <l xml:id="idm140462578513488">mine -,</l>
               <l xml:id="idm140462578513072">og ingen ved det -,</l>
               <l xml:id="idm140462578512656">o herligt -!</l>
            </lg>
         </div>
      </body>
   </text>
