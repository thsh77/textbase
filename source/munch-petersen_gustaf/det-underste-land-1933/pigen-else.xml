   <text>
      <body>
         <div xml:id="workid69095" decls="#biblid69095">
            <head xml:id="idm140462578794928">pigen else</head>
            <div xml:id="idm140462578794512">
               <lg xml:id="idm140462578794256">
                  <l xml:id="idm140462578794000">engang blev du skræmt -</l>
                  <l xml:id="idm140462578793552">nu sidder du med friheden -</l>
                  <l xml:id="idm140462578793120">dit hjærte vejer intet -,</l>
                  <l xml:id="idm140462578792672">o du er fri -</l>
               </lg>
            </div>
            <div xml:id="idm140462578792000">
               <lg xml:id="idm140462578791744">
                  <l xml:id="idm140462578791488">altid holder du dit røgfarvede smil</l>
                  <l xml:id="idm140462578791008">op foran øjnene,</l>
                  <pb n="115" xml:id="idm140462578790576" rend="missing"/>
                  <l xml:id="idm140462578790032">før du ser paa de andre,</l>
                  <l xml:id="idm140462578789584">som ikke kender skrækken</l>
                  <l xml:id="idm140462578789136">for stumme dyr -</l>
               </lg>
               <lg xml:id="idm140462578788592">
                  <l xml:id="idm140462578788336">nu sidder du med friheden -,</l>
                  <l xml:id="idm140462578787904">dine øjne er trætte</l>
                  <l xml:id="idm140462578787456">som en kos nakke</l>
                  <l xml:id="idm140462578787040">af at smile -</l>
               </lg>
            </div>
         </div>
      </body>
