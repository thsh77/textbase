   <text>
      <body>
         <div xml:id="workid67848" decls="#biblid67848">
            <head xml:id="idm140462578954144">latter</head>
            <lg xml:id="idm140462578953728">
               <l xml:id="idm140462578953472">i fortvivlelse</l>
               <l xml:id="idm140462578953056">saarede jeg helligdommen,</l>
               <l xml:id="idm140462578952624">som du med templer og alt</l>
               <l xml:id="idm140462578952192">havde bundet med kærlighedens traad</l>
               <l xml:id="idm140462578951712">under mit hjærte,</l>
               <l xml:id="idm140462578951280">og den faldt -</l>
            </lg>
            <pb n="106" xml:id="idm140462578950736" rend="missing"/>
            <lg xml:id="idm140462578950240">
               <l xml:id="idm140462578949984">blandt lave stumper</l>
               <l xml:id="idm140462578949568">saa jeg dig gaa</l>
               <l xml:id="idm140462578949152">for evigt -</l>
            </lg>
            <lg xml:id="idm140462578948608">
               <l xml:id="idm140462578948352">længe gik jeg</l>
               <l xml:id="idm140462578947920">bærende min daarelatter</l>
               <l xml:id="idm140462578947472">rundt mellem ruinerne -</l>
               <l xml:id="idm140462578947056">mange kom -</l>
               <l xml:id="idm140462578946640">ingen kunde bygge templer -</l>
               <l xml:id="idm140462578946208">mange gav -</l>
               <l xml:id="idm140462578945792">ingen turde tage latteren -</l>
            </lg>
            <lg xml:id="idm140462578945232">
               <l xml:id="idm140462578944976">o gud - gud - gud -</l>
               <l xml:id="idm140462578944560">og hvis du en dag kommer tilbage -,</l>
               <l xml:id="idm140462578944128">kan jeg ikke kende dig -</l>
               <l xml:id="idm140462578943696">ha - ha -</l>
            </lg>
         </div>
      </body>
