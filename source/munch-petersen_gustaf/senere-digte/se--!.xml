   <text>
      <body>
         <div xml:id="workid68754" decls="#biblid68754">
            <head xml:id="idm140381326952560">se -!</head>
            <lg xml:id="idm140381326952144">
               <l xml:id="idm140381326951888">du er ikke ond</l>
               <l xml:id="idm140381326951472">og ikke god,</l>
               <l xml:id="idm140381326951056">ikke vis</l>
               <l xml:id="idm140381326950640">og ikke svag -</l>
               <l xml:id="idm140381326950224">hemmelige fremtidsviljer</l>
               <l xml:id="idm140381326949792">bruger dig -</l>
               <l xml:id="idm140381326949376">alle er vi udvalgte,</l>
               <l xml:id="idm140381326948960">alle skal vi dø -</l>
               <l xml:id="idm140381326948528">viljerne bruger os -</l>
               <pb n="109" xml:id="idm140381326948112" rend="missing"/>
               <l xml:id="idm140381326947616">ingen kan dømmes, </l>
               <l xml:id="idm140381326947184">og ingen er kronet - </l>
               <l xml:id="idm140381326946768">hemmelige fremtidsviljer </l>
               <l xml:id="idm140381326946336">borer sig </l>
               <l xml:id="idm140381326945920">gennem os </l>
               <l xml:id="idm140381326945504">frem -</l>
            </lg>
         </div>
      </body>
