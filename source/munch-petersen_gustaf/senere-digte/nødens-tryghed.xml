   <text>
      <body>
         <div xml:id="workid68619" decls="#biblid68619">
            <head xml:id="idm140381326970080">nødens tryghed</head>
            <lg xml:id="idm140381326969648">
               <l xml:id="idm140381326969392">jeg selv er af mørke </l>
               <l xml:id="idm140381326968944">har altid været - </l>
               <l xml:id="idm140381326968512">en del af mig </l>
               <l xml:id="idm140381326968096">jeg ved ej hvilken </l>
               <l xml:id="idm140381326967680">findes i andre egne - </l>
               <l xml:id="idm140381326967264">den hvisker mig fremmed </l>
               <l xml:id="idm140381326966832">alt jeg behøver at vide - </l>
               <l xml:id="idm140381326966384">den hvisker fra</l>
               <pb n="108" xml:id="idm140381326965968" rend="missing"/>
               <l xml:id="idm140381326965472">egne, langt fra alt mørke,</l>
               <l xml:id="idm140381326965024">den ser mig aldrig</l>
               <l xml:id="idm140381326964608">og min pande bøjet i mørke,</l>
               <l xml:id="idm140381326964144">dens blik er vendt</l>
               <l xml:id="idm140381326963728">mod andre klarere skæbner -</l>
               <l xml:id="idm140381326963264">i nødens bitreste time</l>
               <l xml:id="idm140381326962816">skal en iskold vind</l>
               <l xml:id="idm140381326962400">fra lyse ukendte egne</l>
               <l xml:id="idm140381326961984">hviske mig alt jeg behøver -</l>
            </lg>
         </div>
      </body>
