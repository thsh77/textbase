   <text>
      <body>
         <div xml:id="workid68695" decls="#biblid68695">
            <head xml:id="idm140381326960304">vi stolte</head>
            <lg xml:id="idm140381326959888">
               <l xml:id="idm140381326959632">lad os bøje os,</l>
               <l xml:id="idm140381326959200">vi stolte -,</l>
               <l xml:id="idm140381326958784">lad os søge et haab -</l>
            </lg>
            <lg xml:id="idm140381326958208">
               <l xml:id="idm140381326957952">lad os aabne os,</l>
               <l xml:id="idm140381326957536">vi store -,</l>
               <l xml:id="idm140381326957120">lad os vise vor armod -</l>
            </lg>
            <lg xml:id="idm140381326956576">
               <l xml:id="idm140381326956320">lad os tale,</l>
               <l xml:id="idm140381326955904">vi stærke -,</l>
               <l xml:id="idm140381326955472">lad os dræbe vort had -</l>
            </lg>
            <lg xml:id="idm140381326954896">
               <l xml:id="idm140381326954640">lad os bøje os,</l>
               <l xml:id="idm140381326954208">lad os søge et haab -</l>
            </lg>
         </div>
