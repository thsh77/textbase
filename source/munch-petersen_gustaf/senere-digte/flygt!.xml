   <text>
      <body>
         <div xml:id="workid68820" decls="#biblid68820">
            <head xml:id="idm140381326943872">flygt!</head>
            <lg xml:id="idm140381326943456">
               <l xml:id="idm140381326943200">med hænderne knuget</l>
               <l xml:id="idm140381326942752">i en krampe, haardere end sten,</l>
               <l xml:id="idm140381326942320">med øjnene</l>
               <l xml:id="idm140381326941904">blinde af en gnist,</l>
               <l xml:id="idm140381326941488">som hverken er haab eller glæde,</l>
               <l xml:id="idm140381326941024">med fødderne tvunget af en vilje,</l>
               <l xml:id="idm140381326940560">hinsides baade menneske og fremtid,</l>
               <l xml:id="idm140381326940128">flygt -!</l>
            </lg>
            <lg xml:id="idm140381326939584">
               <l xml:id="idm140381326939328">flygt, skjult af en vildskab,</l>
               <l xml:id="idm140381326938896">som dræber, hader alt,</l>
               <l xml:id="idm140381326938448">som elsker</l>
               <l xml:id="idm140381326938032">eet,</l>
               <l xml:id="idm140381326937616">flygt, frælsende den draabe i dig,</l>
               <l xml:id="idm140381326937152">som livet vilde dø</l>
               <l xml:id="idm140381326936720">uden -</l>
            </lg>
         </div>
      </body>
