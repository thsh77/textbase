   <text>
      <body>
         <div xml:id="workid68955" decls="#biblid68955">
            <head xml:id="idm140381326926304">digt</head>
            <lg xml:id="idm140381326925888">
               <l xml:id="idm140381326925632">jeg er en fattig synder,</l>
               <l xml:id="idm140381326925200">der søger harmoni -</l>
               <l xml:id="idm140381326924752">oh, sollyse drifter og</l>
               <l xml:id="idm140381326924336">skumle lyster og mord</l>
               <l xml:id="idm140381326923920">og pjerrot-løgne -</l>
               <l xml:id="idm140381326923488">solen gaar ned i natten</l>
               <l xml:id="idm140381326923072">og kommer igen med dag -</l>
               <l xml:id="idm140381326922640">i et kammer, i en mands figur</l>
               <l xml:id="idm140381326922208">sidder sjælen med lyse øjne -</l>
               <l xml:id="idm140381326921744">sorgen over livet</l>
               <l xml:id="idm140381326921328">er det dejligste haab -</l>
            </lg>
         </div>
      </body>
   </text>
