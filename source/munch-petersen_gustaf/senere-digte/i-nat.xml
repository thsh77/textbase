   <text>
      <body>
         <div xml:id="workid68888" decls="#biblid68888">
            <head xml:id="idm140381326935088">i nat</head>
            <lg xml:id="idm140381326934672">
               <l xml:id="idm140381326934416">i nat frøs</l>
               <l xml:id="idm140381326934000">tøsneen ikke -</l>
               <l xml:id="idm140381326933568">sirenerne skreg</l>
               <l xml:id="idm140381326933152">i mildhedens taage -</l>
               <l xml:id="idm140381326932736">jorden vældede blød</l>
               <l xml:id="idm140381326932288">frem af drivernes skred -</l>
            </lg>
            <lg xml:id="idm140381326931728">
               <l xml:id="idm140381326931472">luftens isnende tomhed </l>
               <l xml:id="idm140381326931056">fyldes af væde - </l>
               <l xml:id="idm140381326930624">lysende damp</l>
               <pb n="110" xml:id="idm140381326930208" rend="missing"/>
               <l xml:id="idm140381326929712">dækker maanens lys -</l>
               <l xml:id="idm140381326929264">jorden skinner brun</l>
               <l xml:id="idm140381326928848">mod morgenens tøvende magt -</l>
               <l xml:id="idm140381326928384">havet aander blegt,</l>
               <l xml:id="idm140381326927968">før solen rejser sig -</l>
            </lg>
         </div>
      </body>
