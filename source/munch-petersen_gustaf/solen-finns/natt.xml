   <text>
      <body>
         <div xml:id="workid67999" decls="#biblid67999">
            <head xml:id="idm140381327052128">natt</head>
            <lg xml:id="idm140381327051712">
               <l xml:id="idm140381327051456">- himlen mörknar </l>
               <l xml:id="idm140381327051024">ödena samlar sig </l>
               <l xml:id="idm140381327050608">mänskolivet sover</l>
               <l xml:id="idm140381327050176">vårens eller höstens sömn -</l>
               <l xml:id="idm140381327049712">- ödena talar,</l>
               <l xml:id="idm140381327049280">sol och måne väntar -</l>
            </lg>
            <lg xml:id="idm140381327048704">
               <l xml:id="idm140381327048448">- natten vänder sig dröjande </l>
               <l xml:id="idm140381327047984">mänskolivet kvider i sömnen </l>
               <l xml:id="idm140381327047520">ödena väntar</l>
               <l xml:id="idm140381327047104">att skåda verkställelsen -</l>
            </lg>
         </div>
      </body>
   </text>
