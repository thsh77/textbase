   <text>
      <body>
         <div xml:id="workid68049" decls="#biblid68049">
            <head xml:id="idm140381327045376">ord</head>
            <lg xml:id="idm140381327044992">
               <l xml:id="idm140381327044736">inför det stora och enkla</l>
               <l xml:id="idm140381327044288">äro orden dimmiga</l>
               <l xml:id="idm140381327043872">och tonfallet allt -</l>
               <l xml:id="idm140381327043456">inför det falska och oäkta</l>
               <l xml:id="idm140381327042992">fattas mig ord -</l>
               <l xml:id="idm140381327042576">jag trampar med skrovlig häl</l>
               <l xml:id="idm140381327042112">rot, stängel, allt,</l>
               <l xml:id="idm140381327041664">lyftande hat och mord</l>
               <pb n="104" xml:id="idm140381327041248" rend="missing"/>
               <l xml:id="idm140381327040752">som livets kross-spark</l>
               <l xml:id="idm140381327040336">åt döden -</l>
               <l xml:id="idm140381327039920">ångest-blind famlande:</l>
               <l xml:id="idm140381327039504">blomma</l>
               <l xml:id="idm140381327039088">sol</l>
               <l xml:id="idm140381327038656">mänska -</l>
            </lg>
         </div>
      </body>
