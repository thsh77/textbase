      <body>
         <div xml:id="workid64307" decls="#biblid64307">
            <head xml:id="idm140381327556016">mänskoord</head>
            <lg xml:id="idm140381327555600">
               <l xml:id="idm140381327555344">om liv att skänka</l>
               <l xml:id="idm140381327554912">är din längtan,</l>
               <l xml:id="idm140381327554496">tigg icke av ängen blommor,</l>
               <l xml:id="idm140381327554032">av buskarna bär -</l>
               <l xml:id="idm140381327553600">måla knoppar med ditt blod,</l>
               <l xml:id="idm140381327553136">sätt dem på döda kvistar,</l>
               <l xml:id="idm140381327552672">sov framtidens sömn -</l>
            </lg>
         </div>
      </body>
