      <body>
         <div xml:id="workid66100" decls="#biblid66100">
            <head xml:id="idm140381327317120">bortom</head>
            <lg xml:id="idm140381327316704">
               <l xml:id="idm140381327316448">ständigt blixtrar det, fast</l>
               <l xml:id="idm140381327315984">molnen stå fjärran -</l>
               <l xml:id="idm140381327315536">dansa icke,</l>
               <l xml:id="idm140381327315120">fastän det spelas,</l>
               <l xml:id="idm140381327314688">blixtarna väsa bortom</l>
               <l xml:id="idm140381327314240">hoppets och förtvivlans toner,</l>
               <l xml:id="idm140381327313776">dansa icke,</l>
               <l xml:id="idm140381327313360">ständigt blixtrar det -</l>
               <l xml:id="idm140381327312912">molnen stå fjärran,</l>
               <l xml:id="idm140381327312464">bortom förtvivlan och hopp -</l>
            </lg>
         </div>
      </body>
