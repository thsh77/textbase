   <text>
      <body>
         <div xml:id="workid65401" decls="#biblid65401">
            <head xml:id="idm140381327410640">visa icke</head>
            <lg xml:id="idm140381327410224">
               <l xml:id="idm140381327409968">visa icke ditt ansikte</l>
               <l xml:id="idm140381327409552">för världen,</l>
               <l xml:id="idm140381327409120">ditt ansikte är</l>
               <l xml:id="idm140381327408688">för striden,</l>
               <l xml:id="idm140381327408256">ditt ansikte är</l>
               <l xml:id="idm140381327407824">visiret att tecknas av hugg,</l>
               <l xml:id="idm140381327407392">visiret får</l>
               <l xml:id="idm140381327406960">icke fläckas</l>
               <l xml:id="idm140381327406528">med förtidig glädjes bitande fläckar,</l>
               <l xml:id="idm140381327406048">ditt ansikte är fanan att</l>
               <l xml:id="idm140381327405600">ödmjukt lägga, riven</l>
               <l xml:id="idm140381327405184">men ofläckad i barnets hand,</l>
               <l xml:id="idm140381327404720">eller okännbar trampas</l>
               <l xml:id="idm140381327404272">i hopplöshetens seger,</l>
               <l xml:id="idm140381327403824">ditt ansikte är</l>
               <l xml:id="idm140381327403392">för striden,</l>
               <l xml:id="idm140381327402960">visa icke ditt ansikte</l>
               <l xml:id="idm140381327402544">för världen -</l>
            </lg>
         </div>
      </body>
