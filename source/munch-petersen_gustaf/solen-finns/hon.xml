         <div xml:id="workid66538" decls="#biblid66538">
            <head xml:id="idm140381327260464">hon</head>
            <lg xml:id="idm140381327260080">
               <l xml:id="idm140381327259824">- fattig, övermäktig av hat, </l>
               <l xml:id="idm140381327259360">vågade jag språnget från </l>
               <l xml:id="idm140381327258896">fängelsegluggen -, </l>
               <l xml:id="idm140381327258448">naken, jagad till vanvett, </l>
               <l xml:id="idm140381327258016">slukade mig skogen - </l>
               <l xml:id="idm140381327257600">strålande, rik och stolt </l>
               <l xml:id="idm140381327257152">trampade hon</l>
               <l xml:id="idm140381327256736">på min hand, där jag låg,</l>
               <l xml:id="idm140381327256272">och bjöd mig allt, hon ägde</l>
               <l xml:id="idm140381327255808">mot min kärlek -</l>
               <l xml:id="idm140381327255376">jag räckte henne, törstande, min</l>
               <l xml:id="idm140381327254912">nakna uselhet,</l>
               <l xml:id="idm140381327254496">jag blev stark och rik -</l>
               <l xml:id="idm140381327254064">- alla, som jag mötte sedan, </l>
               <l xml:id="idm140381327253600">bad mig om, vad jag skänkt -,</l>
               <l xml:id="idm140381327253136">alla, som jag gav utav min rikedom,</l>
               <l xml:id="idm140381327252704">har dött usla dödar -</l>
               <l xml:id="idm140381327252256">och mina skatter av is och blod</l>
               <l xml:id="idm140381327251824">är outtömliga, och själv</l>
               <l xml:id="idm140381327251392">har jag aldrig vågat smaka -</l>
               <l xml:id="idm140381327250928">- i fasans fjärran skog </l>
               <l xml:id="idm140381327250480">sitter hon,</l>
               <l xml:id="idm140381327250064">saligt lekande med min lycka -</l>
            </lg>
            <pb n="94" xml:id="idm140381327249504" rend="missing"/>
