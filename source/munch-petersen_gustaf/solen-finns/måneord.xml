   <text>
      <body>
         <div xml:id="workid65822" decls="#biblid65822">
            <head xml:id="idm140381327354656">måneord</head>
            <lg xml:id="idm140381327354240">
               <l xml:id="idm140381327353984">molnen jagar -</l>
               <l xml:id="idm140381327353568">månen hänger, tyst räknande</l>
               <l xml:id="idm140381327353104">hästar och huvuden -</l>
               <l xml:id="idm140381327352656">månen vet,</l>
               <l xml:id="idm140381327352240">att hetsande moln</l>
               <l xml:id="idm140381327351824">bara finns stunden,</l>
               <l xml:id="idm140381327351408">då de täcker honom -</l>
               <l xml:id="idm140381327350960">månen vet, att ångesten hatar</l>
               <l xml:id="idm140381327350496">rötter och lökar -</l>
               <l xml:id="idm140381327350048">månen skär tyst</l>
               <l xml:id="idm140381327349616">livets rispa genom jakten -</l>
            </lg>
         </div>
      </body>
