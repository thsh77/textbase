   <text>
      <body>
         <div xml:id="workid64583" decls="#biblid64583">
            <head xml:id="idm140381327518752">teckning</head>
            <lg xml:id="idm140381327518336">
               <l xml:id="idm140381327518080">nej -,</l>
               <l xml:id="idm140381327517664">min stora -,</l>
               <l xml:id="idm140381327517248">grymma -,</l>
               <l xml:id="idm140381327516832">alltförlåtande kärlek</l>
               <l xml:id="idm140381327516384">får</l>
               <l xml:id="idm140381327515968">inte blomma -</l>
               <l xml:id="idm140381327515552">nej -,</l>
               <l xml:id="idm140381327515136">- åh, varför -, varför -</l>
               <l xml:id="idm140381327514688">nej -,</l>
               <l xml:id="idm140381327514272">hon är så obeveklig</l>
               <l xml:id="idm140381327513824">och stor -,</l>
               <l xml:id="idm140381327513408">så löjlig</l>
               <l xml:id="idm140381327512992">och grym - hon är</l>
               <l xml:id="idm140381327512560">så ond,</l>
               <pb n="80" xml:id="idm140381327512144" rend="missing"/>
               <l xml:id="idm140381327511648">hon förlåter</l>
               <l xml:id="idm140381327511216">gränslöst</l>
               <l xml:id="idm140381327510800">allt -,</l>
               <l xml:id="idm140381327510384">jag befaller</l>
               <l xml:id="idm140381327509968">henne att stanna,</l>
               <l xml:id="idm140381327509552">där hon är -</l>
               <l xml:id="idm140381327509120">hon får</l>
               <l xml:id="idm140381327508704">inte</l>
               <l xml:id="idm140381327508288">blomma -</l>
               <l xml:id="idm140381327507872">jag måste</l>
               <l xml:id="idm140381327507456">befalla</l>
               <l xml:id="idm140381327507040">henne att stanna</l>
               <l xml:id="idm140381327506624">där hon är -</l>
            </lg>
         </div>
      </body>
