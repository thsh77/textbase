   <text>
      <body>
         <div xml:id="workid67131" decls="#biblid67131">
            <head xml:id="idm140381327182352">vapen</head>
            <lg xml:id="idm140381327181936">
               <l xml:id="idm140381327181680">stolthet -, tystnad -,</l>
               <l xml:id="idm140381327181264">vapnen, som skyddar</l>
               <l xml:id="idm140381327180848">den enda, längsta</l>
               <l xml:id="idm140381327180416">vägen -</l>
               <l xml:id="idm140381327180000">det finns ingen egen</l>
               <l xml:id="idm140381327179584">seger, ingen egen</l>
               <l xml:id="idm140381327179168">död -,</l>
               <l xml:id="idm140381327178752">det finns bara livets</l>
               <l xml:id="idm140381327178336">nederlag i dig, eller din</l>
               <l xml:id="idm140381327177904">öppenhet utan tvekan, när</l>
               <l xml:id="idm140381327177472">rösterna talar eller krusar havet</l>
               <l xml:id="idm140381327177008">med sina andedräkter -</l>
            </lg>
         </div>
      </body>
