   <text>
      <body>
         <div xml:id="workid67306" decls="#biblid67306">
            <head xml:id="idm140381327159008">törnet</head>
            <lg xml:id="idm140381327158592">
               <l xml:id="idm140381327158336">stör mig icke -</l>
               <l xml:id="idm140381327157904">låt mig icke veta främmande lögner -</l>
               <l xml:id="idm140381327157424">jag älskar icke ensamheten -</l>
               <l xml:id="idm140381327156960">jag älskar ingenting -</l>
               <l xml:id="idm140381327156512">jag ville -</l>
               <l xml:id="idm140381327156096">jag kunde -</l>
               <l xml:id="idm140381327155680">stör mig icke -</l>
               <l xml:id="idm140381327155248">jag måste vara -</l>
               <l xml:id="idm140381327154816">fläcka icke vad, som icke är -</l>
            </lg>
         </div>
      </body>
