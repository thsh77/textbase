   <text>
      <body>
         <div xml:id="workid67075" decls="#biblid67075">
            <head xml:id="idm140381327189728">fienden</head>
            <lg xml:id="idm140381327189312">
               <l xml:id="idm140381327189056">bara segern och vilan äro gemensamma, </l>
               <l xml:id="idm140381327188576">det finns inga gemensamma strider, </l>
               <l xml:id="idm140381327188144">alla strider äro f järran från vilan, </l>
               <l xml:id="idm140381327187664">alla strider ha bara en fiende, </l>
               <l xml:id="idm140381327187232">alla strider brytas i mörker, </l>
               <l xml:id="idm140381327186768">där ingen ser, bara slår</l>
               <pb n="97" xml:id="idm140381327186320" rend="missing"/>
               <l xml:id="idm140381327185776">fienden -</l>
               <l xml:id="idm140381327185360">låt oss dela</l>
               <l xml:id="idm140381327184928">vilans och fröjdens längtan,</l>
               <l xml:id="idm140381327184464">striden måste dansas i mörkret,</l>
               <l xml:id="idm140381327184000">ensam med allas fiende -</l>
            </lg>
         </div>
      </body>
   </text>
