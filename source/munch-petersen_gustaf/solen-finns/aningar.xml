   <text>
      <body>
         <div xml:id="workid66146" decls="#biblid66146">
            <head xml:id="idm140381327310784">aningar</head>
            <lg xml:id="idm140381327310368">
               <l xml:id="idm140381327310112">solen vandrar</l>
               <l xml:id="idm140381327309696">ung, tyst och strålande</l>
               <l xml:id="idm140381327309248">över klipporna, ejdrarna och husen,</l>
               <l xml:id="idm140381327308816">lugnt, säkert -</l>
               <l xml:id="idm140381327308384">sedan lägger hon sig</l>
               <l xml:id="idm140381327307936">som en löftesrik gula</l>
               <l xml:id="idm140381327307488">i havets morgonlovande vita -</l>
            </lg>
            <lg xml:id="idm140381327306928">
               <l xml:id="idm140381327306672">tysta klippor</l>
               <l xml:id="idm140381327306256">moderligt bekymrade ejdrar</l>
               <l xml:id="idm140381327305824">och blinda hus</l>
               <l xml:id="idm140381327305408">diskutera med havet</l>
               <l xml:id="idm140381327304992">mina aningar,</l>
               <l xml:id="idm140381327304576">om de kanske än</l>
               <l xml:id="idm140381327304144">ändå skulle vara sanna -</l>
            </lg>
            <pb n="91" xml:id="idm140381327303584" rend="missing"/>
            <lg xml:id="idm140381327303088">
               <l xml:id="idm140381327302832">båten driver,</l>
               <l xml:id="idm140381327302400">mina armar ha domnat,</l>
               <l xml:id="idm140381327301984">jag viskar</l>
               <l xml:id="idm140381327301568">mot tjärdoftande botten:</l>
               <l xml:id="idm140381327301120">jag tror på tystnaden</l>
               <l xml:id="idm140381327300672">och morgondagen,</l>
               <l xml:id="idm140381327300256">jag hatar det, som man vet -,</l>
               <l xml:id="idm140381327299824">och jag väntar på</l>
               <l xml:id="idm140381327299392">att livet skall lyfta</l>
               <l xml:id="idm140381327298976">mig som en rödgul rova mot solen,</l>
               <l xml:id="idm140381327298512">eller tyst gömma mig</l>
               <l xml:id="idm140381327298064">under dimman som ett misstag -</l>
            </lg>
         </div>
      </body>
