   <text>
      <body>
         <div xml:id="workid64536" decls="#biblid64536">
            <head xml:id="idm140381327525120">jag önskar mig</head>
            <lg xml:id="idm140381327524688">
               <l xml:id="idm140381327524432">okända ansikten,</l>
               <l xml:id="idm140381327524000">att glödande känna igen -,</l>
               <l xml:id="idm140381327523536">ett aldrig brukat leende,</l>
               <l xml:id="idm140381327523104">att ta som en fågel mellan mina händer -,</l>
               <l xml:id="idm140381327522624">en ny ström av blod,</l>
               <l xml:id="idm140381327522176">att ha som ett spett</l>
               <l xml:id="idm140381327521760">av järn för blodets strider -,</l>
               <l xml:id="idm140381327521296">och stoltast</l>
               <l xml:id="idm140381327520880">önskar jag mig den eviga vakan,</l>
               <l xml:id="idm140381327520448">den inget undgår -</l>
            </lg>
         </div>
      </body>
   </text>
