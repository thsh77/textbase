   <text>
      <body>
         <div xml:id="workid64482" decls="#biblid64482">
            <head xml:id="idm140381327532432">höst</head>
            <lg xml:id="idm140381327532016">
               <l xml:id="idm140381327531760">jag ville,</l>
               <l xml:id="idm140381327531344">att livet skulle vara en stark</l>
               <l xml:id="idm140381327530912">röd älskog, iförd hårt lärft,</l>
               <pb n="79" xml:id="idm140381327530448" rend="missing"/>
               <l xml:id="idm140381327529952">en rygg, som tålde att hatas,</l>
               <l xml:id="idm140381327529488">en vilja till annat än smekning -</l>
               <l xml:id="idm140381327529024">men det liv, jag fann,</l>
               <l xml:id="idm140381327528608">var en ödmjuk,</l>
               <l xml:id="idm140381327528176">bedjande rygg,</l>
               <l xml:id="idm140381327527760">en gnista, som ömt måste skyddas,</l>
               <l xml:id="idm140381327527296">ett hopp, för vilket varje åker var mager</l>
               <l xml:id="idm140381327526816">och varje regn vinterns is -</l>
            </lg>
         </div>
      </body>
