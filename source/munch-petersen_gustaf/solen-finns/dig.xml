            <head xml:id="idm140381327237600">
               <ref target="#luta-dig-1" xml:id="idm140381327237472">luta</ref>
               <note xml:id="luta-dig-1" anchored="true">
                  <p xml:id="idm140381327236096">læn</p>
               </note> dig</head>
            <lg xml:id="idm140381327235520">
               <l xml:id="idm140381327235264">luta dig mot mig -</l>
               <l xml:id="idm140381327234848">jag innehåller din vila -,</l>
               <l xml:id="idm140381327234400">jag äger att utstycka din kraft -</l>
               <l xml:id="idm140381327233936">luta dig mot mig -</l>
               <l xml:id="idm140381327233520">jag är ett träd, du</l>
               <l xml:id="idm140381327233072">ett annat -</l>
            </lg>
         </div>
      </body>
