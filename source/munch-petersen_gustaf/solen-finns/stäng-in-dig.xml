   <text>
      <body>
         <div xml:id="workid65528" decls="#biblid65528">
            <head xml:id="idm140381327393952">stäng in dig</head>
            <lg xml:id="idm140381327393520">
               <l xml:id="idm140381327393264">stäng in dig i din kammare</l>
               <l xml:id="idm140381327392816">med din svaghets guld,</l>
               <l xml:id="idm140381327392400">vänta i din kammare,</l>
               <l xml:id="idm140381327391952">tills kärlekens mod har kommit,</l>
               <l xml:id="idm140381327391488">vänta,</l>
               <l xml:id="idm140381327391072">tills ett öppet svärd ligger</l>
               <l xml:id="idm140381327390608">mellan vad du måste och vad du icke</l>
               <l xml:id="idm140381327390128">längre kan -</l>
            </lg>
         </div>
      </body>
   </text>
