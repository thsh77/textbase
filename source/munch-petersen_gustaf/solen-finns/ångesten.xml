   <text>
      <body>
         <div xml:id="workid65333" decls="#biblid65333">
            <head xml:id="idm140381327419536">ångesten</head>
            <lg xml:id="idm140381327419120">
               <l xml:id="idm140381327418864">dra dig undan ångestens</l>
               <l xml:id="idm140381327418416">smala händer, ångesten är helig,</l>
               <l xml:id="idm140381327417952">ångesten gömmer</l>
               <l xml:id="idm140381327417536">ett utav stegen, som behövas -</l>
               <l xml:id="idm140381327417072">ångesten hotar</l>
               <l xml:id="idm140381327416656">alla rodnande händer,</l>
               <pb n="85" xml:id="idm140381327416208" rend="missing"/>
               <l xml:id="idm140381327415712">ångesten vill intet,</l>
               <l xml:id="idm140381327415296">ångesten äger intet,</l>
               <l xml:id="idm140381327414880">ångesten har intet att förlora,</l>
               <l xml:id="idm140381327414448">ångesten hotar</l>
               <l xml:id="idm140381327414032">alla som snuddar utan att vilja -</l>
               <l xml:id="idm140381327413600">dra dig undan</l>
               <l xml:id="idm140381327413184">ångestens smala händer,</l>
               <l xml:id="idm140381327412752">ångesten gömmer</l>
               <l xml:id="idm140381327412336">heligt ett utav stegen som behövas -</l>
            </lg>
         </div>
      </body>
