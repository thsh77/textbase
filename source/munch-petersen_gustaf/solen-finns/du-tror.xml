   <text>
      <body>
         <div xml:id="workid65252" decls="#biblid65252">
            <head xml:id="idm140381327430640">du tror</head>
            <lg xml:id="idm140381327430224">
               <l xml:id="idm140381327429968">du tror att jag gömmer mina skatter</l>
               <l xml:id="idm140381327429488">av hat,</l>
               <l xml:id="idm140381327429072">du tror att jag lyder</l>
               <l xml:id="idm140381327428656">min stolthed, när jag tiger,</l>
               <l xml:id="idm140381327428192">att mitt rus är glädje -</l>
               <l xml:id="idm140381327427744">- det finns dagar som sekunder -, </l>
               <l xml:id="idm140381327427312">det finns dagar, som sparar</l>
               <l xml:id="idm140381327426880">åt livet timmar av blod -</l>
            </lg>
         </div>
      </body>
   </text>
