   <text>
      <body>
         <div xml:id="workid65199" decls="#biblid65199">
            <head xml:id="idm140381327437696">tro</head>
            <lg xml:id="idm140381327437312">
               <l xml:id="idm140381327437056">jag vill följa den eviga blindheten, </l>
               <l xml:id="idm140381327436576">stolt vill jag kyssa hennes piska, </l>
               <l xml:id="idm140381327436144">jag vill ödmjuka mina ögon till</l>
               <pb n="84" xml:id="idm140381327435680" rend="missing"/>
               <l xml:id="idm140381327435136">hatets maskar på den blommande lågheten -,</l>
               <l xml:id="idm140381327434640">jag vill blända min själ, att</l>
               <l xml:id="idm140381327434176">hon må gå säkert -,</l>
               <l xml:id="idm140381327433728">jag vill älska vad jag icke kan fatta,</l>
               <l xml:id="idm140381327433248">och bekämpa allt gott,</l>
               <l xml:id="idm140381327432800">om det finns ett hjärta,</l>
               <l xml:id="idm140381327432352">måste jag hitta det då -</l>
            </lg>
         </div>
      </body>
