   <text>
      <body>
         <div xml:id="workid65713" decls="#biblid65713">
            <head xml:id="idm140381327369200">fjällvatten</head>
            <lg xml:id="idm140381327368768">
               <l xml:id="idm140381327368512">den vita älven </l>
               <l xml:id="idm140381327368080">rinner forsande, </l>
               <l xml:id="idm140381327367664">hatfull förbi det block, </l>
               <l xml:id="idm140381327367216">där en skärva sitter </l>
               <l xml:id="idm140381327366768">som ett öga -</l>
            </lg>
            <lg xml:id="idm140381327366208">
               <l xml:id="idm140381327365952">tag, vita ström, </l>
               <l xml:id="idm140381327365520">allt som vacklar, </l>
               <l xml:id="idm140381327365104">hata, hopplöst sinande, </l>
               <l xml:id="idm140381327364656">din vita grumlighet </l>
               <l xml:id="idm140381327364240">mot blocket -</l>
            </lg>
            <lg xml:id="idm140381327363696">
               <l xml:id="idm140381327363440">när dina sista </l>
               <l xml:id="idm140381327363008">källor äro torra, </l>
               <l xml:id="idm140381327362560">vända sig </l>
               <l xml:id="idm140381327362144">bergets rubiner ut -</l>
            </lg>
            <pb n="88" xml:id="idm140381327361600" rend="missing"/>
         </div>
