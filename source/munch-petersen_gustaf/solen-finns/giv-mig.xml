   <text>
      <body>
         <div xml:id="workid65109" decls="#biblid65109">
            <head xml:id="idm140381327449936">giv mig</head>
            <lg xml:id="idm140381327449520">
               <l xml:id="idm140381327449264">giv mig korv, vin, ost -,</l>
               <l xml:id="idm140381327448832">du ska få en aning</l>
               <l xml:id="idm140381327448400">utav det,</l>
               <l xml:id="idm140381327447984">som gör hungerns bädd</l>
               <l xml:id="idm140381327447536">mjuk någon timme -</l>
               <l xml:id="idm140381327447104">giv mig ett leende -,</l>
               <l xml:id="idm140381327446688">och jag ska ge dig meningen</l>
               <l xml:id="idm140381327446256">med leenden då vi båda</l>
               <l xml:id="idm140381327445808">icke finns -</l>
            </lg>
         </div>
      </body>
   </text>
