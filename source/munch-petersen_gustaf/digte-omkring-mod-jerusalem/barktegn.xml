   <text>
      <body>
         <div xml:id="workid80244" decls="#biblid80244">
            <head xml:id="idm140462577382976">barktegn</head>
            <div xml:id="idm140462577382560">
               <lg xml:id="idm140462577382304">
                  <l xml:id="idm140462577382048">mine aabne øjne:</l>
                  <l xml:id="idm140462577381616">hvad jeg havde set</l>
                  <l xml:id="idm140462577381200">af mig selv -</l>
                  <l xml:id="idm140462577380784">i snehvid dragt rejste</l>
                  <l xml:id="idm140462577380368">jeg mig fra morgenlejet</l>
                  <l xml:id="idm140462577379952">og forkyndte -</l>
                  <l xml:id="idm140462577379536">Ret og u-Ret -</l>
               </lg>
            </div>
            <div xml:id="idm140462577378864">
               <lg xml:id="idm140462577378608">
                  <l xml:id="idm140462577378352">men igaar (hvilket</l>
                  <l xml:id="idm140462577377936">helvede!) sank jeg</l>
                  <l xml:id="idm140462577377520">tilbage (flimrende skovdunkel!)</l>
                  <l xml:id="idm140462577377088">bag</l>
                  <l xml:id="idm140462577376656">øjnene -</l>
               </lg>
            </div>
            <div xml:id="idm140462577375984">
               <lg xml:id="idm140462577375728">
                  <l xml:id="idm140462577375472">mine korte mosgrønne</l>
                  <l xml:id="idm140462577375024">lege i mørket -!</l>
                  <l xml:id="idm140462577374592">de skal</l>
                  <l xml:id="idm140462577374176">frælse eller fortabe -</l>
                  <l xml:id="idm140462577373728">alverden??</l>
                  <l xml:id="idm140462577373312">fluens sol?</l>
                  <l xml:id="idm140462577372896">eller dig?</l>
               </lg>
            </div>
            <div xml:id="idm140462577372224">
               <lg xml:id="idm140462577371968">
                  <l xml:id="idm140462577371712">dog -!</l>
                  <l xml:id="idm140462577371296">mine hemmelige smilende</l>
                  <l xml:id="idm140462577370880">smidige lege</l>
                  <pb n="212" xml:id="idm140462577370464" rend="missing"/>
                  <l xml:id="idm140462577369968">er mig</l>
                  <l xml:id="idm140462577369552">mig i skovens dunkel</l>
                  <l xml:id="idm140462577369136">og min hele kraft -</l>
               </lg>
            </div>
         </div>
      </body>
