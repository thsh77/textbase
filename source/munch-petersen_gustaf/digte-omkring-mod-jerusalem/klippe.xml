   <text>
      <body>
         <div xml:id="workid79739" decls="#biblid79739">
            <head xml:id="idm140462577446688">klippe</head>
            <lg xml:id="idm140462577446272">
               <l xml:id="idm140462577446016">staal - nej haardere,</l>
               <l xml:id="idm140462577445600">haardere - : sten -</l>
               <l xml:id="idm140462577445184">levende - nej ældre,</l>
               <l xml:id="idm140462577444736">ældre - : sult -</l>
               <l xml:id="idm140462577444320">skønhed grimhed - havet aldrig</l>
               <l xml:id="idm140462577443856">forstod mig - :</l>
               <l xml:id="idm140462577443440">intet formaal - !</l>
               <l xml:id="idm140462577443024">højere - nærmere - :</l>
               <l xml:id="idm140462577442576">klippe -</l>
            </lg>
            <lg xml:id="idm140462577442032">
               <l xml:id="idm140462577441776">solen har</l>
               <l xml:id="idm140462577441360">smaa røde tænder -,</l>
               <l xml:id="idm140462577440912">solens visdom - klo -</l>
               <pb n="208" xml:id="idm140462577440496" rend="missing"/>
               <l xml:id="idm140462577440000">kæler en sprække til mit hjærte -</l>
               <l xml:id="idm140462577439520">tomt - tæt - ærligt -</l>
            </lg>
            <lg xml:id="idm140462577438944">
               <l xml:id="idm140462577438688">naar høststormen slikker</l>
               <l xml:id="idm140462577438240">min fod som en kongesko eller</l>
               <l xml:id="idm140462577437808">et trællesaar -,</l>
               <l xml:id="idm140462577437376">da ler mit hjærte,</l>
               <l xml:id="idm140462577436944">da ved jeg,</l>
               <l xml:id="idm140462577436528">at jeg er begrænsning,</l>
               <l xml:id="idm140462577436080">et led,</l>
               <l xml:id="idm140462577435664">en reminiscens -, med</l>
               <l xml:id="idm140462577435248">tusind nye bud</l>
               <l xml:id="idm140462577434832">til at sætte en rødviolet asters</l>
               <l xml:id="idm140462577434368">paa min skurrende pande -</l>
               <l xml:id="idm140462577433936">da forstaar jeg</l>
               <l xml:id="idm140462577433520">sult, flinthjærte, suget og</l>
               <l xml:id="idm140462577433056">forbandelsens ærlighed -:</l>
               <l xml:id="idm140462577432608">klippe - et formaal -</l>
            </lg>
         </div>
