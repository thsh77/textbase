   <text>
      <body>
         <div xml:id="workid78724" decls="#biblid78724">
            <head xml:id="idm140462577572752">koldt, men rent</head>
            <lg xml:id="idm140462577572336">
               <l xml:id="idm140462577572080">jeg har fundet,</l>
               <l xml:id="idm140462577571664">at jeg ikke er fuldendt,</l>
               <l xml:id="idm140462577571232">som jeg havde troet -</l>
               <l xml:id="idm140462577570816">og jeg har fundet,</l>
               <l xml:id="idm140462577570400">at det er uden betydning -</l>
            </lg>
            <lg xml:id="idm140462577569840">
               <l xml:id="idm140462577569584">jeg har fundet,</l>
               <l xml:id="idm140462577569168">at min kærlighed</l>
               <l xml:id="idm140462577568736">til ham, hun eller dem,</l>
               <l xml:id="idm140462577568320">er uvæsentlig -</l>
            </lg>
            <lg xml:id="idm140462577567760">
               <l xml:id="idm140462577567504">jeg har fundet,</l>
               <l xml:id="idm140462577567088">at jeg intet er,</l>
               <l xml:id="idm140462577566672">uden en stemme -</l>
               <l xml:id="idm140462577566256">og at den ikke er min -</l>
            </lg>
            <lg xml:id="idm140462577565712">
               <l xml:id="idm140462577565456">jeg har fundet,</l>
               <l xml:id="idm140462577565040">at "JEG" er et nødvendigt</l>
               <l xml:id="idm140462577564592">stedord -</l>
            </lg>
            <lg xml:id="idm140462577564048">
               <l xml:id="idm140462577563792">- at skabe</l>
               <l xml:id="idm140462577563376">af alles magt i mig</l>
               <pb n="201" xml:id="idm140462577562960" rend="missing"/>
               <l xml:id="idm140462577562464">er min pligt -</l>
               <l xml:id="idm140462577562048">jeg har lært,</l>
               <l xml:id="idm140462577561616">at det reneste redskab</l>
               <l xml:id="idm140462577561200">rammer finest -</l>
            </lg>
         </div>
      </body>
