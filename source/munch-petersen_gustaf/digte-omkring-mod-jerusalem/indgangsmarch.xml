   <text>
      <body>
         <div xml:id="workid79864" decls="#biblid79864">
            <head xml:id="idm140462577430976">indgangsmarch</head>
            <lg xml:id="idm140462577430560">
               <l xml:id="idm140462577430304">vi vandrer frem </l>
               <l xml:id="idm140462577429888">vi vandrer frem </l>
               <l xml:id="idm140462577429472">vandrer alle -</l>
               <l xml:id="idm140462577429056">alle som een </l>
               <l xml:id="idm140462577428640">vandrer vi frem</l>
               <l xml:id="idm140462577428224">halle-luja</l>
               <l xml:id="idm140462577427808">halleluja,</l>
               <l xml:id="idm140462577427392">at se barnet</l>
               <l xml:id="idm140462577426976">som vore </l>
               <l xml:id="idm140462577426560">udtrukne sener </l>
               <l xml:id="idm140462577426144">marterbrændte skød </l>
               <l xml:id="idm140462577425696">flammesorte sind</l>
               <l xml:id="idm140462577425280">har fremfødt</l>
               <l xml:id="idm140462577424848">o født</l>
               <l xml:id="idm140462577424432">o født </l>
               <l xml:id="idm140462577424016">at se barnet</l>
               <l xml:id="idm140462577423600">vandrer vi frem</l>
               <pb n="209" xml:id="idm140462577423184" rend="missing"/>
               <l xml:id="idm140462577422640">som skal frælse os</l>
               <l xml:id="idm140462577422208">alle -</l>
               <l xml:id="idm140462577421792">spytte os i ansigtet</l>
               <l xml:id="idm140462577421376">græde saft i vore</l>
               <l xml:id="idm140462577420944">munde</l>
               <l xml:id="idm140462577420528">o barnet</l>
               <l xml:id="idm140462577420112">barnet -</l>
               <l xml:id="idm140462577419696">se!</l>
               <l xml:id="idm140462577419264">le imod os</l>
               <l xml:id="idm140462577418848">le imod os</l>
               <l xml:id="idm140462577418432">barn vi knæler -</l>
               <l xml:id="idm140462577418000">lidelse, fødsel</l>
               <l xml:id="idm140462577417568">nye kinder resultat</l>
               <l xml:id="idm140462577417152">o se!</l>
               <l xml:id="idm140462577416736">paa barnets dun-</l>
               <l xml:id="idm140462577416320">flagrende vinger</l>
               <l xml:id="idm140462577415904">bæres vi</l>
               <l xml:id="idm140462577415488">alle med vore sorte sind</l>
               <l xml:id="idm140462577415056">lukt i livet - halle-luja</l>
               <l xml:id="idm140462577414624">o halle-luja!</l>
            </lg>
