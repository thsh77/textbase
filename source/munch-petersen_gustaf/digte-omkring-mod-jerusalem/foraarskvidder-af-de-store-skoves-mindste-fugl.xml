   <text>
      <body>
         <div xml:id="workid79555" decls="#biblid79555">
            <head xml:id="idm140462577470224">foraarskvidder
af de store skoves mindste fugl</head>
            <lg xml:id="idm140462577469776">
               <l xml:id="idm140462577469520">stien, bagt over guds tunge,</l>
               <l xml:id="idm140462577469088">smyger sig opefter - fint fint -</l>
               <l xml:id="idm140462577468656">sitrende naale - gult skind - skygger -</l>
            </lg>
            <lg xml:id="idm140462577468096">
               <l xml:id="idm140462577467840">nubret følehorn i par</l>
               <l xml:id="idm140462577467392">tier dunkelt</l>
               <l xml:id="idm140462577466976">med vejs ende i plirefingers solblodnegl -</l>
               <l xml:id="idm140462577466528">o fuldendt -</l>
            </lg>
            <lg xml:id="idm140462577465984">
               <l xml:id="idm140462577465728">men lysende pensler og blind orms øje</l>
               <l xml:id="idm140462577465248">sætter gnist til fjern</l>
               <l xml:id="idm140462577464800">vandrende myretue -</l>
            </lg>
            <lg xml:id="idm140462577464256">
               <l xml:id="idm140462577464000">tag fjeren, som</l>
               <l xml:id="idm140462577463584">lækat har bidt</l>
               <l xml:id="idm140462577463152">tegnet i -</l>
               <l xml:id="idm140462577462736">dit liv begynder</l>
               <l xml:id="idm140462577462320">umiddelbart, spændende, listigt -</l>
               <l xml:id="idm140462577461856">stormens blad planter</l>
               <l xml:id="idm140462577461440">ny hugtand i hjærtets løg -</l>
               <l xml:id="idm140462577460976">og stor bjørn dør -</l>
               <l xml:id="idm140462577460528">- perlesjæl spræller henrykt</l>
               <l xml:id="idm140462577460064">mellem himmel og jord -</l>
               <l xml:id="idm140462577459648">kom du ud nu?</l>
            </lg>
            <pb n="207" xml:id="idm140462577459104" rend="missing"/>
         </div>
      </body>
