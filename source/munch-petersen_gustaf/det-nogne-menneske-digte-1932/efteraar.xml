   <text>
      <body>
         <div xml:id="workid60565" decls="#biblid60565">
            <head xml:id="idm140462579872992">efteraar</head>
            <lg xml:id="idm140462579872576">
               <l xml:id="idm140462579872320">det regner - regner -</l>
               <l xml:id="idm140462579871904">luften er graa og mælket,</l>
               <l xml:id="idm140462579871456">gaderne vaade og kolde,</l>
               <l xml:id="idm140462579871040">menneskene blege</l>
               <l xml:id="idm140462579870624">og klamme som syge fugle,</l>
               <l xml:id="idm140462579870192">jeg gaar og gaar,</l>
               <pb n="25" xml:id="idm140462579869776" rend="missing"/>
               <l xml:id="idm140462579869280">men det bare regner,</l>
               <l xml:id="idm140462579868864">haabløst -</l>
               <l xml:id="idm140462579868448">himlen staar som en mørk mur</l>
               <l xml:id="idm140462579867984">mellem os og solen</l>
               <l xml:id="idm140462579867568">og varmen.</l>
               <l xml:id="idm140462579867152">i husene</l>
               <l xml:id="idm140462579866736">ruller folk gardinerne ned</l>
               <l xml:id="idm140462579866304">og tænder lys -</l>
               <l xml:id="idm140462579865872">paa gaden</l>
               <l xml:id="idm140462579865456">slaar mændene kraven op</l>
               <l xml:id="idm140462579865008">og staar stille,</l>
               <l xml:id="idm140462579864592">saa at tøjet ikke rører ved kroppen-</l>
               <l xml:id="idm140462579864112">det er efteraar.</l>
            </lg>
         </div>
      </body>
