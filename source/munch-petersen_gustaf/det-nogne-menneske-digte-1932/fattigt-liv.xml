   <text>
      <body>
         <div xml:id="workid57821" decls="#biblid57821">
            <head xml:id="idm140462580223296">fattigt liv</head>
            <lg xml:id="idm140462580222880">
               <l xml:id="idm140462580222624">der er dem,</l>
               <l xml:id="idm140462580222208">der hver nat hviler paa samme gren</l>
               <l xml:id="idm140462580221776">og kun spiser af det rette fad.</l>
            </lg>
            <lg xml:id="idm140462580221216">
               <l xml:id="idm140462580220960">der er dem,</l>
               <l xml:id="idm140462580220544">der hver morgen synger den samme sang</l>
               <l xml:id="idm140462580220112">og altid til samme elskede,</l>
               <l xml:id="idm140462580219680">altid kun til een gud.</l>
            </lg>
            <pb n="6" xml:id="idm140462580219136" rend="missing"/>
            <lg xml:id="idm140462580218592">
               <l xml:id="idm140462580218336">der er dem,</l>
               <l xml:id="idm140462580217920">der kun følger een vej over jorden;</l>
               <l xml:id="idm140462580217440">de frygter kun een død</l>
               <l xml:id="idm140462580216992">og de lever kun eet liv.</l>
            </lg>
         </div>
      </body>
