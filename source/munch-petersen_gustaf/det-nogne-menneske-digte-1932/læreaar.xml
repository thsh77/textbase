   <text>
      <body>
         <div xml:id="workid60198" decls="#biblid60198">
            <head xml:id="idm140462579920560">læreaar</head>
            <lg xml:id="idm140462579920144">
               <l xml:id="idm140462579919888">vore nætter</l>
               <l xml:id="idm140462579919456">druknede i vinens sang</l>
               <l xml:id="idm140462579919040">og kvindernes skød -</l>
            </lg>
            <lg xml:id="idm140462579918464">
               <l xml:id="idm140462579918208">vi haabede,</l>
               <l xml:id="idm140462579917792">at kraften vilde komme til os</l>
               <l xml:id="idm140462579917360">som en herreløs hund</l>
               <l xml:id="idm140462579916912">og lægge sig ved vore fødder -</l>
            </lg>
            <lg xml:id="idm140462579916320">
               <l xml:id="idm140462579916064">til nogle af os kom længselen -</l>
               <l xml:id="idm140462579915600">den rev vore skæbner løs</l>
               <l xml:id="idm140462579915152">og bandt dem som trofæer ved sit bælte -</l>
            </lg>
         </div>
      </body>
   </text>
