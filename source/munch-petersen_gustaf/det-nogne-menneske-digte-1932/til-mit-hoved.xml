   <text>
      <body>
         <div xml:id="workid60099" decls="#biblid60099">
            <head xml:id="idm140462579933136">til mit hoved</head>
            <lg xml:id="idm140462579932720">
               <l xml:id="idm140462579932464">jeg er et barn,</l>
               <l xml:id="idm140462579932048">der leger med sit legetøj -</l>
               <l xml:id="idm140462579931584">mit legetøj:</l>
               <l xml:id="idm140462579931152">en bold med øjne i -</l>
            </lg>
            <lg xml:id="idm140462579930576">
               <l xml:id="idm140462579930320">jeg triller den</l>
               <l xml:id="idm140462579929904">og kaster den mod væggen -</l>
               <l xml:id="idm140462579929456">jeg klapper den</l>
               <l xml:id="idm140462579929040">og smider den i vandet -</l>
            </lg>
            <lg xml:id="idm140462579928480">
               <l xml:id="idm140462579928224">maaske tilsidst</l>
               <l xml:id="idm140462579927808">mit legetøj blev ødelagt -</l>
               <l xml:id="idm140462579927344">men legetøj</l>
               <l xml:id="idm140462579926912">har aldrig holdt saa længe -</l>
            </lg>
            <lg xml:id="idm140462579926320">
               <l xml:id="idm140462579926064">maaske tilsidst</l>
               <l xml:id="idm140462579925648">jeg tabte det i græsset -</l>
               <l xml:id="idm140462579925200">det har jeg glemt,</l>
               <l xml:id="idm140462579924784">hvor det blev af tilsidst -</l>
            </lg>
            <pb n="22" xml:id="idm140462579924224" rend="missing"/>
            <lg xml:id="idm140462579923680">
               <l xml:id="idm140462579923424">maaske engang</l>
               <l xml:id="idm140462579923008">jeg finder det alligevel -</l>
               <l xml:id="idm140462579922576">mit legetøj,</l>
               <l xml:id="idm140462579922144">hvis det er helt endnu -</l>
            </lg>
         </div>
      </body>
