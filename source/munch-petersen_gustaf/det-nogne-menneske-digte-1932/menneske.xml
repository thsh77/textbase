   <text>
      <body>
         <div xml:id="workid58478" decls="#biblid58478">
            <head xml:id="idm140462580138512">menneske</head>
            <lg xml:id="idm140462580138096">
               <l xml:id="idm140462580137840">menneske -</l>
               <l xml:id="idm140462580137424">om du er stærk, og din stemme høres af mange,</l>
               <l xml:id="idm140462580136928">eller du er lille og svag,</l>
               <l xml:id="idm140462580136496">om du er dum eller klog,</l>
               <l xml:id="idm140462580136064">hvad er det mod alt det andet</l>
               <l xml:id="idm140462580135632">i verden -</l>
               <l xml:id="idm140462580135216">intet -</l>
               <l xml:id="idm140462580134800">bjærgene er høje og lave,</l>
               <l xml:id="idm140462580134352">roserne er røde og duftende,</l>
               <l xml:id="idm140462580133888">men ogsaa visne og gule -</l>
               <l xml:id="idm140462580133456">det er ikke det -</l>
               <l xml:id="idm140462580133040">men</l>
               <l xml:id="idm140462580132656">at du kan vælte dig i nydelser</l>
               <l xml:id="idm140462580132192">og midt i rusen græde taarer,</l>
               <l xml:id="idm140462580131728">men</l>
               <l xml:id="idm140462580131344">at du kan lide</l>
               <l xml:id="idm140462580130928">og af din smerte gøre en hyldestsang til livet</l>
            </lg>
            <pb n="11" xml:id="idm140462580130304" rend="missing"/>
         </div>
