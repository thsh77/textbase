   <text>
      <body>
         <div xml:id="workid61703" decls="#biblid61703">
            <head xml:id="idm140462579728384">fröding</head>
            <lg xml:id="idm140462579727968">
               <l xml:id="idm140462579727712">din stille latter</l>
               <l xml:id="idm140462579727296">gjorde andres nagende synder</l>
               <l xml:id="idm140462579726864">og forspildte liv</l>
               <l xml:id="idm140462579726448">til munter dans paa solgrønne enge-</l>
            </lg>
            <lg xml:id="idm140462579725840">
               <l xml:id="idm140462579725584">jeg ser den fortvivlede kamp,</l>
               <l xml:id="idm140462579725152">som rev dit eget sind i laser - ,</l>
            </lg>
            <lg xml:id="idm140462579724592">
               <l xml:id="idm140462579724336">og jeg tier -</l>
            </lg>
            <lg xml:id="idm140462579723792">
               <l xml:id="idm140462579723536">
                  <hi rend="italics" xml:id="idm140462579723280">vi er mennesker</hi>
               </l>
               <l xml:id="idm140462579722464">
                  <hi rend="italics" xml:id="idm140462579722208">hvorfor bebrejde hinanden,</hi>
               </l>
               <l xml:id="idm140462579721376">
                  <hi rend="italics" xml:id="idm140462579721120">at vi ikke er andet -</hi>
               </l>
            </lg>
         </div>
      </body>
