   <text>
      <body>
         <div xml:id="workid61416" decls="#biblid61416">
            <head xml:id="idm140462579765872">en tak</head>
            <lg xml:id="idm140462579765456">
               <l xml:id="idm140462579765200">kære -,</l>
               <l xml:id="idm140462579764784">jeg ved ikke, om dit sind bærer skjulte saar,</l>
               <pb n="31" xml:id="idm140462579764288" rend="missing"/>
               <l xml:id="idm140462579763792">jeg kender ikke dine lyse og mørke timer,</l>
               <l xml:id="idm140462579763312">ikke dine hverdage -,</l>
            </lg>
            <lg xml:id="idm140462579762768">
               <l xml:id="idm140462579762512">men dine smil -</l>
            </lg>
            <lg xml:id="idm140462579761968">
               <l xml:id="idm140462579761712">du tog min uro,</l>
               <l xml:id="idm140462579761296">du gav hvile til mit søgende legeme -</l>
               <l xml:id="idm140462579760816">af dig fik jeg arbejdets kraft tilbage - ,</l>
            </lg>
            <lg xml:id="idm140462579760240">
               <l xml:id="idm140462579759984">og jeg fik dine smil</l>
               <l xml:id="idm140462579759568">den nat -</l>
            </lg>
         </div>
      </body>
