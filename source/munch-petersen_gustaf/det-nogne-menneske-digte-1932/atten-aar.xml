   <text>
      <body>
         <div xml:id="workid58872" decls="#biblid58872">
            <head xml:id="idm140462580088896">atten aar</head>
            <lg xml:id="idm140462580088480">
               <l xml:id="idm140462580088224">mand og kvinde -</l>
               <l xml:id="idm140462580087808">sort firkantet sofa</l>
               <pb n="13" xml:id="idm140462580087392" rend="missing"/>
               <l xml:id="idm140462580086848">med blege blomster -</l>
               <l xml:id="idm140462580086432">det store lys er slukket -</l>
               <l xml:id="idm140462580086000">en svag lampe brænder</l>
               <l xml:id="idm140462580085552">gult -</l>
            </lg>
            <lg xml:id="idm140462580085008">
               <l xml:id="idm140462580084752">de kysser hinanden</l>
               <l xml:id="idm140462580084336">længe -</l>
               <l xml:id="idm140462580083920">de knuger hinanden</l>
               <l xml:id="idm140462580083504">haardt -</l>
               <l xml:id="idm140462580083088">som om der var noget,</l>
               <l xml:id="idm140462580082672">der ikke maatte være -</l>
            </lg>
            <lg xml:id="idm140462580082096">
               <l xml:id="idm140462580081840">hun har bare skuldre -</l>
               <l xml:id="idm140462580081424">bryster</l>
               <l xml:id="idm140462580081008">igennem tyndt grønt tøj -</l>
               <l xml:id="idm140462580080560">han piller kælent</l>
               <l xml:id="idm140462580080128">ved underkjolen -</l>
               <l xml:id="idm140462580079712">hun blir rød og alvorlig:</l>
               <l xml:id="idm140462580079264">hvor det er svært at være</l>
               <l xml:id="idm140462580078816">ung -</l>
            </lg>
            <lg xml:id="idm140462580078272">
               <l xml:id="idm140462580078016">maa ikke -</l>
               <l xml:id="idm140462580077600">tør ikke -</l>
               <l xml:id="idm140462580077184">han kysser længe -</l>
               <l xml:id="idm140462580076752">smiler:</l>
               <l xml:id="idm140462580076336">det er der jo ikke noget at gøre ved -</l>
               <l xml:id="idm140462580075856">hun sukker -</l>
            </lg>
            <lg xml:id="idm140462580075312">
               <l xml:id="idm140462580075056">og saa var der ikke mere -</l>
            </lg>
         </div>
      </body>
