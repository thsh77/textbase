   <text>
      <body>
         <div xml:id="workid60337" decls="#biblid60337">
            <head xml:id="idm140462579902432">nat</head>
            <lg xml:id="idm140462579902048">
               <l xml:id="idm140462579901792">om natten tvivler vi -</l>
            </lg>
            <lg xml:id="idm140462579901248">
               <l xml:id="idm140462579900992">smaa svage mennesker</l>
               <l xml:id="idm140462579900576">i store tomme huse -</l>
               <l xml:id="idm140462579900160">der er for god plads om natten -</l>
               <l xml:id="idm140462579899728">vi kan ikke fylde m√∏rket</l>
               <l xml:id="idm140462579899280">med kendte ting -</l>
            </lg>
            <lg xml:id="idm140462579898736">
               <l xml:id="idm140462579898480">angsten</l>
               <l xml:id="idm140462579898064">kryber som rotter</l>
               <l xml:id="idm140462579897648">med lange kolde haler</l>
               <l xml:id="idm140462579897232">frem af sorte huler -</l>
               <l xml:id="idm140462579896816">angst for det intet,</l>
               <l xml:id="idm140462579896400">som vi ikke kender,</l>
               <l xml:id="idm140462579895984">ikke ved, er intet -</l>
            </lg>
            <lg xml:id="idm140462579895440">
               <l xml:id="idm140462579895184">hvor er mennesker smaa</l>
               <l xml:id="idm140462579894768">om natten -</l>
            </lg>
         </div>
      </body>
