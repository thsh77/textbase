   <text>
      <body>
         <div xml:id="workid58213" decls="#biblid58213">
            <head xml:id="idm140462580172560">alligevel</head>
            <lg xml:id="idm140462580172144">
               <l xml:id="idm140462580171888">jeg vaklede rundt i mørke,</l>
               <l xml:id="idm140462580171440">og jeg frøs.</l>
            </lg>
            <lg xml:id="idm140462580170880">
               <l xml:id="idm140462580170624">jeg havde slaaet dem,</l>
               <l xml:id="idm140462580170208">jeg holdt af.</l>
            </lg>
            <lg xml:id="idm140462580169664">
               <l xml:id="idm140462580169408">jeg havde stjaalet fra dem,</l>
               <l xml:id="idm140462580168976">der vilde mig vel.</l>
            </lg>
            <lg xml:id="idm140462580168432">
               <l xml:id="idm140462580168176">jeg var ond og grim,</l>
               <l xml:id="idm140462580167760">og ingen kvinde vilde smile til mig.</l>
            </lg>
            <lg xml:id="idm140462580167200">
               <l xml:id="idm140462580166944">og jeg saa paa mine urene hænder</l>
               <l xml:id="idm140462580166480">og græd over mit forspildte liv.</l>
            </lg>
            <pb n="9" xml:id="idm140462580165888" rend="missing"/>
            <lg xml:id="idm140462580165392">
               <l xml:id="idm140462580165136">men solen</l>
               <l xml:id="idm140462580164720">spredte snavset paa mit ansigt</l>
            </lg>
            <lg xml:id="idm140462580164160">
               <l xml:id="idm140462580163904">og jeg saa op,</l>
               <l xml:id="idm140462580163488">og jeg saa, at jeg levede.</l>
            </lg>
         </div>
      </body>
