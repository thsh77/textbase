   <text>
      <body>
         <div xml:id="workid58423" decls="#biblid58423">
            <head xml:id="idm140462580145952">lisbeth</head>
            <lg xml:id="idm140462580145536">
               <l xml:id="idm140462580145280">du er en lille jumbe</l>
               <l xml:id="idm140462580144864">med bjælder paa -</l>
               <l xml:id="idm140462580144432">muntert trippende hest -</l>
               <l xml:id="idm140462580144000">solen glitrer</l>
               <l xml:id="idm140462580143584">i kuskens røde fjæs.</l>
               <l xml:id="idm140462580143136">hvide syrener -</l>
               <l xml:id="idm140462580142720">glade mennesker</l>
               <l xml:id="idm140462580142304">snadrer -</l>
               <l xml:id="idm140462580141888">mænd og cigarer,</l>
               <l xml:id="idm140462580141456">koner med kaffe</l>
               <l xml:id="idm140462580141040">og thermosflasker -</l>
               <l xml:id="idm140462580140624">søndag.</l>
               <l xml:id="idm140462580140208">selvfølgeligt.</l>
            </lg>
         </div>
      </body>
