   <text>
      <body>
         <div xml:id="workid59715" decls="#biblid59715">
            <head xml:id="idm140462579981312">døden</head>
            <lg xml:id="idm140462579980896">
               <l xml:id="idm140462579980640">hvorfor vi lever -</l>
               <l xml:id="idm140462579980224">ja -</l>
               <pb n="19" xml:id="idm140462579979808" rend="missing"/>
               <l xml:id="idm140462579979312">det hænder, at vi er saa glade, at vi synger -</l>
               <l xml:id="idm140462579978816">maaske blir vi saa lykkelige, saa glade,</l>
               <l xml:id="idm140462579978368">at vi engang stille gaar vores vej,</l>
               <l xml:id="idm140462579977936">af angst for at vi ikke mer skal høre livets unge latter -</l>
            </lg>
         </div>
      </body>
   </text>
