   <text>
      <body>
         <div xml:id="workid61280" decls="#biblid61280">
            <head xml:id="idm140462579783536">generationer</head>
            <lg xml:id="idm140462579783120">
               <l xml:id="idm140462579782864">ordene rakte sig</l>
               <l xml:id="idm140462579782448">som beredvillige hænder imod os:</l>
               <pb n="30" xml:id="idm140462579781984" rend="missing"/>
               <l xml:id="idm140462579781488">vent</l>
               <l xml:id="idm140462579781072">se, i er unge,</l>
               <l xml:id="idm140462579780656">se jeres smalle skuldre,</l>
               <l xml:id="idm140462579780224">jeres ufærdige træk -</l>
               <l xml:id="idm140462579779776">se livet staar undseeligt</l>
               <l xml:id="idm140462579779344">som en fremmed blandt jer -</l>
               <l xml:id="idm140462579778912">hør jeres tankers hurtige skridt,</l>
               <l xml:id="idm140462579778448">naar de flygter for hinanden,</l>
               <l xml:id="idm140462579778016">og vent -</l>
               <l xml:id="idm140462579777600">lader i nu jeres ord</l>
               <l xml:id="idm140462579777184">storme ud over verden,</l>
               <l xml:id="idm140462579776768">for at splitte menneskenes ro,</l>
               <l xml:id="idm140462579776336">i morgen vil i forgæves lokke dem tilbage-</l>
            </lg>
            <lg xml:id="idm140462579775728">
               <l xml:id="idm140462579775472">vi tvivlede -</l>
            </lg>
         </div>
      </body>
