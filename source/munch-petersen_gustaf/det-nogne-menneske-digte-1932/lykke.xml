      <body>
         <div xml:id="workid60863" decls="#biblid60863">
            <head xml:id="idm140462579838496">lykke</head>
            <lg xml:id="idm140462579838080">
               <l xml:id="idm140462579837824">- lykken -,</l>
               <l xml:id="idm140462579837408">siger menneskene -</l>
               <l xml:id="idm140462579836992">spiser sig mætte</l>
               <l xml:id="idm140462579836560">og lægger sig to og to -</l>
            </lg>
            <lg xml:id="idm140462579835984">
               <l xml:id="idm140462579835728">- lykke -,</l>
               <l xml:id="idm140462579835312">klager de -</l>
               <l xml:id="idm140462579834896">og forsøger at drømme sig mætte</l>
               <l xml:id="idm140462579834432">og varme -</l>
            </lg>
            <pb n="27" xml:id="idm140462579833888" rend="missing"/>
            <lg xml:id="idm140462579833392">
               <l xml:id="idm140462579833136">- lykke -,</l>
               <l xml:id="idm140462579832720">raser menneskene -</l>
               <l xml:id="idm140462579832304">og maddyngerne kravler</l>
               <l xml:id="idm140462579831888">fra den ene side til den anden -</l>
            </lg>
            <lg xml:id="idm140462579831328">
               <l xml:id="idm140462579831072">- gudinde -</l>
               <l xml:id="idm140462579830656">giv mig kamp - :</l>
               <l xml:id="idm140462579830240">sejr eller nederlag -,</l>
               <l xml:id="idm140462579829824">og maaske ogsaa</l>
               <l xml:id="idm140462579829408">lykke -</l>
            </lg>
         </div>
      </body>
