   <text>
      <body>
         <div xml:id="workid58099" decls="#biblid58099">
            <head xml:id="idm140462580187008">banalt</head>
            <lg xml:id="idm140462580186592">
               <l xml:id="idm140462580186336">firkantet kvinde</l>
               <l xml:id="idm140462580185920">i et vindue -</l>
               <l xml:id="idm140462580185504">grønt gummiforklæde -</l>
               <l xml:id="idm140462580185056">sværmeriske øjne</l>
               <l xml:id="idm140462580184624">ser ned paa en lille sort hund</l>
               <l xml:id="idm140462580184192">ved en lygtepæl. -</l>
               <l xml:id="idm140462580183760">bare blaa arme</l>
               <l xml:id="idm140462580183344">af og til</l>
               <pb n="8" xml:id="idm140462580182928" rend="missing"/>
               <l xml:id="idm140462580182384">er hun bøjet</l>
               <l xml:id="idm140462580181952">over en hvid kold blank spand -</l>
               <l xml:id="idm140462580181520">hun vrider kluden -</l>
               <l xml:id="idm140462580181104">hvidt sæbevand</l>
               <l xml:id="idm140462580180672">staar i fine haarde straaler</l>
               <l xml:id="idm140462580180240">ud mellem røde fingre,</l>
               <l xml:id="idm140462580179792">som mælk af en kos patter -</l>
               <l xml:id="idm140462580179328">kluden er en chemise,</l>
               <l xml:id="idm140462580178912">den øverste halvdel af en. -</l>
               <l xml:id="idm140462580178448">hvid chemise over hvide skuldre -</l>
               <l xml:id="idm140462580178016">himmelblaa kittel har hun. -</l>
               <l xml:id="idm140462580177584">noget af haaret er faldet ned i panden</l>
               <l xml:id="idm140462580177152">ruderne er snart blanke;</l>
               <l xml:id="idm140462580176720">hun ser igennem dem.</l>
               <l xml:id="idm140462580176304">stolt -</l>
               <l xml:id="idm140462580175888">saa glider hun bort fra vinduet</l>
               <l xml:id="idm140462580175456">med et blødt hop</l>
               <l xml:id="idm140462580175024">ind bagved -</l>
               <l xml:id="idm140462580174608">bare et nypudset vindue</l>
               <l xml:id="idm140462580174192">overfor.</l>
            </lg>
         </div>
