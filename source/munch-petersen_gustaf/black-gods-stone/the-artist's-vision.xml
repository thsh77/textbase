            <head xml:id="idm140381328699184">the artist's vision</head>
            <div xml:id="idm140381328698768">
               <lg xml:id="idm140381328698512">
                  <l xml:id="idm140381328698256">I know </l>
                  <l xml:id="idm140381328697840">everything - </l>
                  <l xml:id="idm140381328697424">I have payed </l>
                  <l xml:id="idm140381328697008">everything -</l>
               </lg>
            </div>
            <div xml:id="idm140381328696336">
               <lg xml:id="idm140381328696080">
                  <l xml:id="idm140381328695824">there is no </l>
                  <l xml:id="idm140381328695408">heart - </l>
                  <l xml:id="idm140381328694992">there is no </l>
                  <l xml:id="idm140381328694576">sun -</l>
                  <l xml:id="idm140381328694160">there is no </l>
                  <l xml:id="idm140381328693744">life -</l>
               </lg>
               <lg xml:id="idm140381328693200">
                  <l xml:id="idm140381328692944">there is</l>
                  <l xml:id="idm140381328692528">the god of hardness -</l>
                  <l xml:id="idm140381328692112">there is</l>
                  <l xml:id="idm140381328691696">the goddess of stone -</l>
                  <l xml:id="idm140381328691280">there is</l>
                  <l xml:id="idm140381328690864">the ice of my will -</l>
               </lg>
            </div>
         </div>
