   <text>
      <body>
         <div xml:id="workid61508" decls="#biblid61508">
            <head xml:id="idm140381327906992">the ring</head>
            <lg xml:id="idm140381327906576">
               <l xml:id="idm140381327906320">I have broken the floor </l>
               <l xml:id="idm140381327905888">of my loneliness -</l>
               <pb n="55" xml:id="idm140381327905472" rend="missing"/>
               <l xml:id="idm140381327904928">I dig the claws of my soul </l>
               <l xml:id="idm140381327904496">deep into the rock - </l>
               <l xml:id="idm140381327904080">downwards, downwards -</l>
            </lg>
            <lg xml:id="idm140381327903536">
               <l xml:id="idm140381327903280">the rock of darkness</l>
               <l xml:id="idm140381327902864">has devoured me -</l>
               <l xml:id="idm140381327902448">all directions</l>
               <l xml:id="idm140381327902032">but one are torn asunder -</l>
               <l xml:id="idm140381327901600">downwards, downwards -!</l>
            </lg>
            <lg xml:id="idm140381327901056">
               <l xml:id="idm140381327900800">I have broken the floor</l>
               <l xml:id="idm140381327900384">of my loneliness -</l>
               <l xml:id="idm140381327899968">softly, softly</l>
               <l xml:id="idm140381327899552">I sink through the mountains</l>
               <l xml:id="idm140381327899120">of blindness -</l>
            </lg>
            <lg xml:id="idm140381327898576">
               <l xml:id="idm140381327898320">soon</l>
               <l xml:id="idm140381327897904">I shall eat the scent</l>
               <l xml:id="idm140381327897488">of the hearts of my brothers -</l>
               <l xml:id="idm140381327897056">soon my drop</l>
               <l xml:id="idm140381327896640">can't be distinguished</l>
               <l xml:id="idm140381327896224">from those of my brothers -</l>
               <l xml:id="idm140381327895792">soon the river</l>
               <l xml:id="idm140381327895376">will close the ring of future -</l>
            </lg>
         </div>
      </body>
