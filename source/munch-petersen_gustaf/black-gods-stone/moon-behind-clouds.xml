   <text>
      <body>
         <div xml:id="workid56494" decls="#biblid56494">
            <head xml:id="idm140381328531728">moon behind clouds</head>
            <lg xml:id="idm140381328531312">
               <l xml:id="idm140381328531056">find your god,</l>
               <l xml:id="idm140381328530640">or you can't live -!</l>
               <l xml:id="idm140381328530224">the gods are waiting</l>
               <l xml:id="idm140381328529808">motionless inflexible -</l>
               <l xml:id="idm140381328529392">the black god</l>
               <l xml:id="idm140381328528976">of the eagle is standing,</l>
               <l xml:id="idm140381328528544">his dark, closed face turned away,</l>
               <l xml:id="idm140381328528112">waiting -</l>
            </lg>
            <pb n="21" xml:id="idm140381328527568" rend="missing"/>
            <lg xml:id="idm140381328527072">
               <l xml:id="idm140381328526816">the red god of the snake,</l>
               <l xml:id="idm140381328526384">coiling with locked lips,</l>
               <l xml:id="idm140381328525952">ready, ready -</l>
               <l xml:id="idm140381328525536">the green god</l>
               <l xml:id="idm140381328525120">of the grass with</l>
               <l xml:id="idm140381328524704">silent blades,</l>
               <l xml:id="idm140381328524288">awake, awake -</l>
               <l xml:id="idm140381328523872">the unseen god,</l>
               <l xml:id="idm140381328523456">watching for the blood to walk,</l>
               <l xml:id="idm140381328523024">claiming, claiming -</l>
               <l xml:id="idm140381328522608">find your god -,</l>
               <l xml:id="idm140381328522192">or you can't live -!</l>
            </lg>
            <lg xml:id="idm140381328521648">
               <l xml:id="idm140381328521392">you may worship the pale breed</l>
               <l xml:id="idm140381328520960">of the selves -,</l>
               <l xml:id="idm140381328520544">pace, begging, towards the smiling</l>
               <l xml:id="idm140381328520112">woman o f death -</l>
               <l xml:id="idm140381328519696">- the gods</l>
               <l xml:id="idm140381328519280">are waiting,</l>
               <l xml:id="idm140381328518864">waiting -!</l>
            </lg>
         </div>
      </body>
