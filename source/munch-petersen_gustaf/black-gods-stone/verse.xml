   <text>
      <body>
         <div xml:id="workid58261" decls="#biblid58261">
            <head xml:id="idm140381328313344">verse</head>
            <lg xml:id="idm140381328312928">
               <l xml:id="idm140381328312672">I wanted to love, </l>
               <l xml:id="idm140381328312256">rage came to me - </l>
               <l xml:id="idm140381328311840">I wanted to hate, </l>
               <l xml:id="idm140381328311424">everything weakened -</l>
            </lg>
            <pb n="33" xml:id="idm140381328310880" rend="missing"/>
            <lg xml:id="idm140381328310384">
               <l xml:id="idm140381328310128">I wanted to leave,</l>
               <l xml:id="idm140381328309712">there came</l>
               <l xml:id="idm140381328309296">a sad-eyed girl, she took my hand,</l>
               <l xml:id="idm140381328308864">I slept -</l>
               <l xml:id="idm140381328308448">I have not risen -</l>
            </lg>
         </div>
      </body>
