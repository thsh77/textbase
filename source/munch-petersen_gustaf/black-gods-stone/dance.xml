   <text>
      <body>
         <div xml:id="workid57929" decls="#biblid57929">
            <head xml:id="idm140381328355264">dance</head>
            <lg xml:id="idm140381328354848">
               <l xml:id="idm140381328354592">- the hour </l>
               <l xml:id="idm140381328354176">is</l>
               <l xml:id="idm140381328353792">there -</l>
               <l xml:id="idm140381328353376">the hour will cover the souls -</l>
            </lg>
            <lg xml:id="idm140381328352816">
               <l xml:id="idm140381328352560">- the hour </l>
               <l xml:id="idm140381328352144">is</l>
               <l xml:id="idm140381328351760">there -</l>
               <l xml:id="idm140381328351344">everything burns </l>
               <l xml:id="idm140381328350928">everything is open -</l>
               <l xml:id="idm140381328350512">- the hour -</l>
               <l xml:id="idm140381328350096">- the choice -</l>
               <l xml:id="idm140381328349680">and the silent dagger -</l>
               <l xml:id="idm140381328349264">- the hour -</l>
               <l xml:id="idm140381328348848">- the hour -</l>
            </lg>
            <lg xml:id="idm140381328348304">
               <l xml:id="idm140381328348048">everything will kill - </l>
               <l xml:id="idm140381328347632">everything will die -</l>
               <l xml:id="idm140381328347216">- the choice -</l>
               <l xml:id="idm140381328346800">- the hour - </l>
               <l xml:id="idm140381328346384">- everything will live - </l>
               <l xml:id="idm140381328345952">- everything will bear -</l>
               <pb n="31" xml:id="idm140381328345520" rend="missing"/>
               <l xml:id="idm140381328345024">- the choice -</l>
               <l xml:id="idm140381328344608">- the hour -</l>
            </lg>
            <lg xml:id="idm140381328344064">
               <l xml:id="idm140381328343808">the hour has covered the souls - </l>
               <l xml:id="idm140381328343376">the hour will stab the voices - </l>
               <l xml:id="idm140381328342944">the choice </l>
               <l xml:id="idm140381328342528">will love -</l>
            </lg>
            <lg xml:id="idm140381328341984">
               <l xml:id="idm140381328341728">- the hour </l>
               <l xml:id="idm140381328341312">is there -</l>
            </lg>
         </div>
