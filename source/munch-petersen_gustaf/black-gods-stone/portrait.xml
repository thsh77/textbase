   <text>
      <body>
         <div xml:id="workid55012" decls="#biblid55012">
            <head xml:id="idm140381328679696">portrait</head>
            <lg xml:id="idm140381328679280">
               <l xml:id="idm140381328679024">when sleeping</l>
               <l xml:id="idm140381328678608">I seek my princess -,</l>
            </lg>
            <lg xml:id="idm140381328678064">
               <l xml:id="idm140381328677808">in the morning</l>
               <l xml:id="idm140381328677392">I break all the flowers -,</l>
            </lg>
            <lg xml:id="idm140381328676832">
               <l xml:id="idm140381328676576">in the sun</l>
               <l xml:id="idm140381328676160">I build my aloneness -,</l>
            </lg>
            <lg xml:id="idm140381328675616">
               <l xml:id="idm140381328675360">towards night</l>
               <l xml:id="idm140381328674944">I carve out the future -,</l>
            </lg>
            <lg xml:id="idm140381328674384">
               <l xml:id="idm140381328674128">my life I spare -</l>
               <l xml:id="idm140381328673712">my death</l>
               <l xml:id="idm140381328673296">shall never exist -</l>
            </lg>
         </div>
      </body>
