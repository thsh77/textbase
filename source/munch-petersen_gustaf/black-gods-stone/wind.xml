   <text>
      <body>
         <div xml:id="workid54938" decls="#biblid54938">
            <head xml:id="idm140381328689168">wind</head>
            <lg xml:id="idm140381328688752">
               <l xml:id="idm140381328688496">my love is the merciless killer -</l>
               <l xml:id="idm140381328688064">my love is sailing in blood -</l>
               <l xml:id="idm140381328687632">my love can only subdue -</l>
               <l xml:id="idm140381328687200">my love can't like the subdued -</l>
               <l xml:id="idm140381328686768">my love doesn't want no more blood -</l>
            </lg>
            <lg xml:id="idm140381328686208">
               <l xml:id="idm140381328685952">on the rock</l>
               <l xml:id="idm140381328685536">the far-off eyes of the albatross</l>
               <l xml:id="idm140381328685104">is waiting -</l>
               <l xml:id="idm140381328684688">the reef</l>
               <l xml:id="idm140381328684272">is not the last -</l>
               <l xml:id="idm140381328683856">the goal is one only -</l>
               <l xml:id="idm140381328683440">that is</l>
               <l xml:id="idm140381328683024">what the wind must bring -</l>
               <l xml:id="idm140381328682592">soon -</l>
               <l xml:id="idm140381328682176">the wind only</l>
               <l xml:id="idm140381328681760">is eternal -</l>
            </lg>
            <pb n="13" xml:id="idm140381328681216" rend="missing"/>
         </div>
