         <div xml:id="workid59076" decls="#biblid59076">
            <head xml:id="idm140111864781440">
               <hi rend="italics" xml:id="idm140111864781184">ET FORAARSBUD</hi>
            </head>
            <lg xml:id="idm140111864780368">
               <l xml:id="idm140111864780112">Et Foraarsbud, en Vaarblomst</l>
               <l xml:id="idm140111864779680">har atter kysset mig.</l>
               <l xml:id="idm140111864779264">Og kær og ung og sælsom</l>
               <l xml:id="idm140111864778816">er min Kærlighed til dig.</l>
            </lg>
            <lg xml:id="idm140111864778240">
               <l xml:id="idm140111864777984">Og intet har en Skærhed</l>
               <l xml:id="idm140111864777536">som min ømme Drøm om dig,</l>
               <l xml:id="idm140111864777088">ak, unge, fine Vaarblomst</l>
               <l xml:id="idm140111864776656">langt renere end jeg.</l>
            </lg>
            <lg xml:id="idm140111864776112">
               <l xml:id="idm140111864775856">Gud Sol! lad Vaaren leve,</l>
               <l xml:id="idm140111864775424">lad med sin lyse Streg</l>
               <l xml:id="idm140111864775008">det unge Foraar spire!</l>
               <l xml:id="idm140111864774592">__ __ __ __</l>
               <l xml:id="idm140111864774176">det unge Foraar spire.</l>
            </lg>
         </div>
