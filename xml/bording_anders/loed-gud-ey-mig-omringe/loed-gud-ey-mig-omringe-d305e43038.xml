      <body>
         <div xml:id="workid136268" decls="#biblid136268">
            <head xml:id="idm140114460787696">206.<lb xml:id="idm140114460787408"/>
               <hi rend="italics" xml:id="idm140114460787152">In Festo Michaelis.</hi>
            </head>
            <lg xml:id="idm140114460786336">
               <head xml:id="idm140114460786080">1.</head>
               <l xml:id="idm140114460785696">Loed Gud ey mig omringe</l>
               <l xml:id="idm140114460785280">Med Englers tryge vagt</l>
               <l xml:id="idm140114460784864">Hvo kunde da betvinge</l>
               <l xml:id="idm140114460784448">Den grumme Sathans magt</l>
               <l xml:id="idm140114460784032">Gaf hand ey deres skare</l>
               <l xml:id="idm140114460783616">Befalning at staae bj</l>
               <l xml:id="idm140114460783200">Hvo kunde mig forsvare</l>
               <l xml:id="idm140114460782784">Och for u-lykken frje.</l>
            </lg>
            <lg xml:id="idm140114460782240">
               <head xml:id="idm140114460781984">2.</head>
               <l xml:id="idm140114460781600">Min egen magt er ringe</l>
            </lg>
         </div>
      </body>
