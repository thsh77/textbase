   <text>
      <body>
         <div xml:id="workid120332" decls="#biblid120332">
            <head xml:id="idm140571804733616">
               <hi rend="bold" xml:id="idm140571804733360">
                  <hi rend="italics" xml:id="idm140571804732832">Rachab.</hi>
               </hi>
               <lb xml:id="idm140571804732016"/>
               <hi rend="bold" xml:id="idm140571804731760">
                  <hi rend="italics" xml:id="idm140571804731232">No. 41. *)</hi>
               </hi>
            </head>
            <lg xml:id="idm140571804730288">
               <l xml:id="idm140571804730032">Om I gik ud ad Porten,</l>
               <l xml:id="idm140571804729616">Som nys blev lukt i Laas!</l>
               <l xml:id="idm140571804729184">De sagtens ned til Floden</l>
               <l xml:id="idm140571804728752">Dog sætte deres Kaas.</l>
            </lg>
            <lg xml:id="idm140571804728176">
               <l xml:id="idm140571804727920">Nu gode Raad er dyre,</l>
               <l xml:id="idm140571804727504">Hun sagde vennehuld,</l>
               <l xml:id="idm140571804727088">Min Hjelp jeg dog tilbyder</l>
               <l xml:id="idm140571804726656">For Gunst og ei for Guld.</l>
            </lg>
            <lg xml:id="idm140571804726096">
               <l xml:id="idm140571804725840">Om eders Gud den Stærke</l>
               <l xml:id="idm140571804725392">Jeg hørde store Ord,</l>
               <l xml:id="idm140571804724944">Han raade skal alene</l>
               <l xml:id="idm140571804724528">I Himmel og paa Jord!</l>
            </lg>
            <lg xml:id="idm140571804723984">
               <l xml:id="idm140571804723728">Man sang alt for min Vugge,</l>
               <l xml:id="idm140571804723296">At i det røde Hav</l>
               <l xml:id="idm140571804722864">Sit Folk en Vei Han laved,</l>
               <l xml:id="idm140571804722432">Kong
                        <hi rend="italics" xml:id="idm140571804722128">Pharao</hi>
en Grav!!</l>
            </lg>
            <lg xml:id="idm140571804721152">
               <l xml:id="idm140571804720896">Og hvad vi veed tilvisse,</l>
               <l xml:id="idm140571804720464">Det er ei heller smaat:</l>
               <l xml:id="idm140571804720048">For eder maatte bukke</l>
               <l xml:id="idm140571804719632">Selv
                        <hi rend="italics" xml:id="idm140571804719328">Basans</hi>
Kæmpe-Drot.</l>
            </lg>
            <note anchored="true" xml:id="idm140571804718336">*) Efter Josvab. 2 og Math. 1.</note>
            <pb n="416" xml:id="s416" facs="adl/grundtvig/grundtvig12/grun12416"/>
            <lg xml:id="idm140571804716432">
               <l xml:id="idm140571804716176">For eder Bjerge bæve,</l>
               <l xml:id="idm140571804715728">Hvor kan da Byer staae!</l>
               <l xml:id="idm140571804715312">Og disse Mure styrte</l>
               <l xml:id="idm140571804714896">Jeg alt i Drømme saae.</l>
            </lg>
         </div>
      </body>
   </text>
