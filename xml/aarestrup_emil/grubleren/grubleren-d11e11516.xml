   <text>
      <body>
         <div xml:id="workid77252" decls="#biblid77252">
            <head xml:id="idm140382754764624">GRUBLEREN.</head>
            <lg xml:id="idm140382754764208">
               <head xml:id="idm140382754763952">I</head>
               <l xml:id="idm140382754763568">Lad mig sige</l>
               <l xml:id="idm140382754763152">Dig Besked</l>
               <l xml:id="idm140382754762736">Om mit Hjertes</l>
               <l xml:id="idm140382754762320">Lønlighed.</l>
            </lg>
            <lg xml:id="idm140382754761776">
               <head xml:id="idm140382754761520">II</head>
               <l xml:id="idm140382754761136">Hvad mig glæder,</l>
               <l xml:id="idm140382754760704">Hvad med Lyst</l>
               <l xml:id="idm140382754760288">Oftest fylder</l>
               <l xml:id="idm140382754759872">Dette Bryst.</l>
            </lg>
            <lg xml:id="idm140382754759328">
               <head xml:id="idm140382754759072">III</head>
               <l xml:id="idm140382754758688">Ingen Tummel,</l>
               <l xml:id="idm140382754758272">Ingen Glands;</l>
               <l xml:id="idm140382754757856">Ikke Ærens</l>
               <l xml:id="idm140382754757440">Døde Krands.</l>
            </lg>
            <lg xml:id="idm140382754756880">
               <head xml:id="idm140382754756624">IV</head>
               <l xml:id="idm140382754756240">Ikke Venskabs</l>
               <l xml:id="idm140382754755824">Milde Dyd;</l>
               <l xml:id="idm140382754755408">Ikke Elskovs</l>
               <l xml:id="idm140382754754992">Drukne Fryd.</l>
            </lg>
            <lg xml:id="idm140382754754448">
               <head xml:id="idm140382754754192">V</head>
               <l xml:id="idm140382754753808">Hvad mig glæder,</l>
               <l xml:id="idm140382754753376">Hvad med Lyst</l>
               <l xml:id="idm140382754752960">Oftest fylder</l>
               <l xml:id="idm140382754752544">Dette Bryst:</l>
            </lg>
            <pb n="224" facs="adl/aarestrup/aare1/aare1224" xml:id="idm140382754752000"/>
            <lg xml:id="idm140382754751184">
               <head xml:id="idm140382754750928">VI</head>
               <l xml:id="idm140382754750544">Er den stille,</l>
               <l xml:id="idm140382754750128">Er den tro</l>
               <l xml:id="idm140382754749712">Eftertankes</l>
               <l xml:id="idm140382754749296">Dybe Ro.</l>
            </lg>
         </div>
      </body>
