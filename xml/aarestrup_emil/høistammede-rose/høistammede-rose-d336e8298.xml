            <lg xml:id="idm140352654759120">
               <l xml:id="idm140352654758864">Høistammede Rose</l>
               <l xml:id="idm140352654758432">Med Læberne bløde</l>
               <l xml:id="idm140352654758000">Det blæser og regner</l>
               <l xml:id="idm140352654757552">Du slanke du søde</l>
            </lg>
            <lg xml:id="idm140352654756992">
               <l xml:id="idm140352654756736">O lad mig blot være</l>
               <l xml:id="idm140352654756288">Den Stok, der er drevet</l>
               <l xml:id="idm140352654755872">Til Støtte i Jorden</l>
            </lg>
