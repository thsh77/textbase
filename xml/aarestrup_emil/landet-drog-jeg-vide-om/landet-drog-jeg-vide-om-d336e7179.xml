      <body>
         <div xml:id="workid68114" decls="#biblid68114">
            <head xml:id="idm140352655034240">LANDET DROG JEG VIDE OM</head>
            <lg xml:id="idm140352655033824">
               <l xml:id="idm140352655033568">Landet drog jeg vide om</l>
               <l xml:id="idm140352655033152">I min fattige Hue</l>
               <l xml:id="idm140352655032736">Gav mig Gud en Mand,</l>
               <l xml:id="idm140352655032320">Blev jeg en fornem Frue.</l>
               <l xml:id="idm140352655031888"> Gik jeg under Lide</l>
               <l xml:id="idm140352655031472">Vilde baade Peer og Povl</l>
               <l xml:id="idm140352655031040">Navnet paa Manden vide.</l>
               <l xml:id="idm140352655030624">Ødeland hedder min Mand,</l>
               <l xml:id="idm140352655030192">Næsegruus hedder mit Huus,</l>
               <l xml:id="idm140352655029744">Tyg i Ro hedder min Ko,</l>
               <l xml:id="idm140352655029328">Tryne fin hedder mit Sviin</l>
               <l xml:id="idm140352655028896">Fattig paa Haar hedder mit Faar</l>
               <l xml:id="idm140352655028464">Vrikkemaas hedder min Gaas</l>
               <l xml:id="idm140352655028032">Snadderivand hedder min And</l>
               <l xml:id="idm140352655027600">Karl for sin Hat hedder min Kat</l>
               <l xml:id="idm140352655027168">Bieffemund hedder min Hund</l>
               <l xml:id="idm140352655026736">Patpaasut hedder min Glut</l>
               <l xml:id="idm140352655026304">Reentforskarn hedder mit Barn</l>
               <l xml:id="idm140352655025872">Pokker er løs min Tjenestetøs</l>
               <l xml:id="idm140352655025408">Skruegal hedder min Ka'l</l>
               <l xml:id="idm140352655024976">Kirkefane hedder min Hane</l>
               <l xml:id="idm140352655024544">Sorte Hoppe hedder min Loppe</l>
               <l xml:id="idm140352655024112">Bær dit Kors hedder mit Hors</l>
               <l xml:id="idm140352655023648">Lad mig syde hedder min Gryde</l>
               <l xml:id="idm140352655023216">Perlebønne hedder min Høne</l>
               <l xml:id="idm140352655022752">Silkelok hedder min Rok</l>
               <l xml:id="idm140352655022336">See hvor smuk hedder min Buk</l>
               <l xml:id="idm140352655021904">Hjelp, vor Herre! hedder min Kerre</l>
               <l xml:id="idm140352655021472">Sommerlæ hedder mit Træ.</l>
            </lg>
         </div>
