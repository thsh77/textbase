   <text>
      <body>
         <div xml:id="workid63146" decls="#biblid63146">
            <head xml:id="idm139983155196288">NAAR JEG I EENRUM FÆRDES</head>
            <lg xml:id="idm139983155195840">
               <head xml:id="idm139983155195584">I</head>
               <l xml:id="idm139983155195200">Naar jeg i Eenrum færdes,</l>
               <l xml:id="idm139983155194752">Med stille Grubien vanker -</l>
               <l xml:id="idm139983155194320">Da er min Sjæl lysvaagen,</l>
               <l xml:id="idm139983155193872">Da sværme mine Tanker.</l>
            </lg>
            <lg xml:id="idm139983155193296">
               <head xml:id="idm139983155193040">II</head>
               <l xml:id="idm139983155192656">Fordobblet min Bevidsthed</l>
               <l xml:id="idm139983155192224">Det smukke Virvar maaler,</l>
               <l xml:id="idm139983155191792">Som det Uendelige</l>
               <l xml:id="idm139983155191376">Omkring min Pande straaler.</l>
            </lg>
            <lg xml:id="idm139983155190816">
               <head xml:id="idm139983155190560">III</head>
               <l xml:id="idm139983155190176">I Skjønheds underfulde</l>
               <l xml:id="idm139983155189728">Mysterier bevandret</l>
               <l xml:id="idm139983155189312">Jeg har Dit Væsen hos mig,</l>
               <l xml:id="idm139983155188864">Frigjordt, men ei forandret.</l>
            </lg>
            <lg xml:id="idm139983155188304">
               <head xml:id="idm139983155188048">IV</head>
               <l xml:id="idm139983155187664">Let flagrer Psychevingen</l>
               <l xml:id="idm139983155187232">Paa Dine Skulderblade,</l>
               <l xml:id="idm139983155186816">Og Dine Lokker slynges</l>
               <l xml:id="idm139983155186400">Udødelige, glade.</l>
            </lg>
            <lg xml:id="idm139983155185840">
               <head xml:id="idm139983155185584">V</head>
               <l xml:id="idm139983155185200">Du tager mig ved Haanden</l>
               <l xml:id="idm139983155184768">Som Dantes Beatrice,</l>
               <l xml:id="idm139983155184352">Og vil mig Himlens Engle</l>
               <l xml:id="idm139983155183920">Og Saligheden vise.</l>
            </lg>
            <pb n="104" facs="adl/aarestrup/aare4/aare4104" xml:id="idm139983155183376"/>
            <lg xml:id="idm139983155182608">
               <head xml:id="idm139983155182352">VI</head>
               <l xml:id="idm139983155181968">Kun naar en Ven mig møder</l>
               <l xml:id="idm139983155181520">Og, som han troer, opliver -</l>
               <l xml:id="idm139983155181088">Og naar jeg Selskabskredsen,</l>
               <l xml:id="idm139983155180656">Som man har sagt, henriver -</l>
            </lg>
            <lg xml:id="idm139983155180096">
               <head xml:id="idm139983155179840">VII</head>
               <l xml:id="idm139983155179456">Saa er det som om Livet</l>
               <l xml:id="idm139983155179040">Fordulgt tilbagestrømmer -</l>
               <l xml:id="idm139983155178592">Det saakaldt Virkelige,</l>
               <l xml:id="idm139983155178176">Det troer jeg, at jeg drømmer.</l>
            </lg>
            <lg xml:id="idm139983155177584">
               <head xml:id="idm139983155177328">VIII</head>
               <l xml:id="idm139983155176912">Det vexler og forsvinder,</l>
               <l xml:id="idm139983155176480">Det hyller sig i Taager -</l>
               <l xml:id="idm139983155176048">Da er det, Sjælen blunder</l>
               <l xml:id="idm139983155175600">Og Legemet kun vaager.</l>
            </lg>
         </div>
