   <text>
      <body>
         <div xml:id="workid71325" decls="#biblid71325">
            <head xml:id="idm140352654634000">VI SAD I VINTERNATTEN</head>
            <lg xml:id="idm140352654633584">
               <l xml:id="idm140352654633328">Vi sad i Vinternatten</l>
               <l xml:id="idm140352654632912">Hvor Ovnen sparsomt gløded;</l>
               <l xml:id="idm140352654632448">Vi holdt i vore Arme</l>
               <l xml:id="idm140352654632032">Det syge Barn paa Skjødet.</l>
            </lg>
            <lg xml:id="idm140352654631456">
               <l xml:id="idm140352654631200">Jeg følte Pulsen svinde</l>
               <l xml:id="idm140352654630752">I Gluttens spæde Aarer;</l>
               <l xml:id="idm140352654630304">Fra dine Øine perle</l>
               <l xml:id="idm140352654629856">Saae jeg de friske Taarer.</l>
            </lg>
            <lg xml:id="idm140352654629296">
               <l xml:id="idm140352654629040">Jeg saae den lille Strube</l>
               <l xml:id="idm140352654628608">De sidste Luftdrag tømme;</l>
            </lg>
         </div>
      </body>
   </text>
