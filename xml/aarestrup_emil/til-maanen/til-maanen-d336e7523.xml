   <text>
      <body>
         <div xml:id="workid68770" decls="#biblid68770">
            <head xml:id="idm140352654952560">TIL MAANEN.</head>
            <lg xml:id="idm140352654952144">
               <head xml:id="idm140352654951888">I</head>
               <l xml:id="idm140352654951504">Jeg har seet ham ride -</l>
               <l xml:id="idm140352654951088">Ha, hvor Hesten jog!</l>
               <l xml:id="idm140352654950672">Til det Sted, jeg boede,</l>
               <l xml:id="idm140352654950240">Ei sit Blik han slog.</l>
            </lg>
            <lg xml:id="idm140352654949696">
               <head xml:id="idm140352654949440">II</head>
               <l xml:id="idm140352654949056">Jeg har seet ham skrive</l>
               <l xml:id="idm140352654948640">Ak, hvor langt det blev!</l>
               <l xml:id="idm140352654948208">Ei til mig var Brevet,</l>
               <l xml:id="idm140352654947792">Han saa ivrig skrev.</l>
            </lg>
            <lg xml:id="idm140352654947248">
               <head xml:id="idm140352654946992">III</head>
               <l xml:id="idm140352654946608">Jeg har seet ham kysse</l>
               <l xml:id="idm140352654946192">Paa en Lok saa blød -</l>
               <l xml:id="idm140352654945744">Ei min Haarlok var det,</l>
               <l xml:id="idm140352654945328">Som den Lykke nød.</l>
            </lg>
            <lg xml:id="idm140352654944768">
               <head xml:id="idm140352654944512">IV</head>
               <l xml:id="idm140352654944128">Jeg har seet ham græde -</l>
               <l xml:id="idm140352654943680">Ak, hvor bleg og skjøn!</l>
               <l xml:id="idm140352654943232">Ei for min Skyld var det,</l>
               <l xml:id="idm140352654942800">At han græd i Løn.</l>
            </lg>
            <lg xml:id="idm140352654942224">
               <head xml:id="idm140352654941968">V</head>
               <l xml:id="idm140352654941584">Jeg har seet ham vandre -</l>
               <l xml:id="idm140352654941152">Hvor han standsed tidt!</l>
               <l xml:id="idm140352654940736">Men det Navn, han sukked,</l>
               <l xml:id="idm140352654940304">Ei det Navn var mit.</l>
            </lg>
            <pb n="153" facs="adl/aarestrup/aare2/aare2153" xml:id="idm140352654939760"/>
            <lg xml:id="idm140352654938992">
               <head xml:id="idm140352654938736">VI</head>
               <l xml:id="idm140352654938352">O, men du - Veninde!</l>
               <l xml:id="idm140352654937936">Klare Maane! - O</l>
               <l xml:id="idm140352654937520">Dine blege Straaler</l>
               <l xml:id="idm140352654937104">Gav mit Hjerte Ro</l>
            </lg>
            <lg xml:id="idm140352654936560">
               <head xml:id="idm140352654936304">VII</head>
               <l xml:id="idm140352654935920">Du har seet min Taare</l>
               <l xml:id="idm140352654935504">Du har seet min Qval</l>
               <l xml:id="idm140352654935088">Naar du stille lyste</l>
               <l xml:id="idm140352654934672">Over Bjerg og Dal</l>
            </lg>
            <lg xml:id="idm140352654934128">
               <head xml:id="idm140352654933872">VIII</head>
               <l xml:id="idm140352654933456">Du har hørt mig sukke</l>
               <l xml:id="idm140352654933008">Paa din høie Borg -</l>
               <l xml:id="idm140352654932560">Som dit Næ forsvinder,</l>
               <l xml:id="idm140352654932112">Svinde lad min Sorg!</l>
            </lg>
         </div>
      </body>
