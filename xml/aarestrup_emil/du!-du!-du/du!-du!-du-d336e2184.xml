   <text>
      <body>
         <div xml:id="workid57999" decls="#biblid57999">
            <head xml:id="idm140352656289568">DU! DU! DU</head>
            <lg xml:id="idm140352656289152">
               <l xml:id="idm140352656288896">Du! Du! Du Søde!</l>
               <l xml:id="idm140352656288464">Du Smekkre! Hvide!</l>
               <l xml:id="idm140352656288048">Du Længselsvarme!</l>
               <l xml:id="idm140352656287616">Som ved min Side,</l>
               <l xml:id="idm140352656287200">Som tidt I mine Arme</l>
               <l xml:id="idm140352656286784">Med kjælne Øielaage,</l>
               <l xml:id="idm140352656286336">Med Blik</l>
               <l xml:id="idm140352656285920">Slukt af en salig Taage,</l>
               <l xml:id="idm140352656285488">Forstemmende af Lykke,</l>
               <l xml:id="idm140352656285072">Og kysforgiftet, næsten døde -</l>
               <l xml:id="idm140352656284608">O jeg forsmægter!</l>
               <l xml:id="idm140352656284176">Jeg kan Din runde Hals ei trykke!</l>
               <l xml:id="idm140352656283744">Jeg tørster efter</l>
               <l xml:id="idm140352656283312">Din Aandes Blomsterdrik,</l>
               <l xml:id="idm140352656282880">Din Arms Omfavningskræfter,</l>
               <l xml:id="idm140352656282416">Din slangebygte Ryg,</l>
               <l xml:id="idm140352656282000">Din Tunges sagte Kys, Din Munds -</l>
               <l xml:id="idm140352656281568">Ha, hvad mig Skjæbnen nægter! - -</l>
               <l xml:id="idm140352656281104">Ak, følte jeg dog kuns</l>
               <l xml:id="idm140352656280656">Din Haands, Din lille Fingers Tryk!</l>
            </lg>
         </div>
      </body>
   </text>
