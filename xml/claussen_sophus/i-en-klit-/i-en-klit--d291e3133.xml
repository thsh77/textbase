            <lg xml:id="idm140264664710592">
               <l xml:id="idm140264664710336">I en Klit</l>
               <l xml:id="idm140264664709920">langt fra mit og dit</l>
               <l xml:id="idm140264664709504">vil vi mødes</l>
               <l xml:id="idm140264664709072">vil vi glemme, at vi ødes</l>
               <l xml:id="idm140264664708624">og vor Kraft er slidt</l>
            </lg>
            <lg xml:id="idm140264664708080">
               <l xml:id="idm140264664707824">i en sandren, sammenføgen</l>
               <l xml:id="idm140264664707376">øde Klit.</l>
            </lg>
