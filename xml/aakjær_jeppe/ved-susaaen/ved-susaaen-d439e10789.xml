   <text>
      <body>
         <div xml:id="workid75152" decls="#biblid75152">
            <head xml:id="idm139850575630848">
               <hi rend="italics" xml:id="idm139850575630720"><!--<emph>-->VED SUSAAEN<!--</emph--></hi>.</head>
            <lg xml:id="idm139850575629552">
               <l xml:id="idm139850575629296">DET var ved Susaaens Vande,</l>
               <l xml:id="idm139850575628864"> du tripped saa ung og sund,</l>
               <l xml:id="idm139850575628432">med Aftenens Guld paa din Pande</l>
               <l xml:id="idm139850575628000">og Kyssets Dug paa din Mund.</l>
            </lg>
            <pb n="109" xml:id="s109" facs="adl/aakjaer/aakjaer2/aakj2109"/>
            <lg xml:id="idm139850575626272">
               <l xml:id="idm139850575626016">Der var i de danske Skove</l>
               <l xml:id="idm139850575625584">ej Rønnebær mere rød</l>
               <l xml:id="idm139850575625136">end dine Læber var røde</l>
               <l xml:id="idm139850575624688">hin Aften mod Nedgangens Glød.</l>
            </lg>
            <lg xml:id="idm139850575624096">
               <l xml:id="idm139850575623840">Det var ved Susaaens Vande,</l>
               <l xml:id="idm139850575623408">jeg husker ej Sted eller Stund,</l>
               <l xml:id="idm139850575622976">kun Lysets Leg paa din Pande</l>
               <l xml:id="idm139850575622544">og Kyssets Dug paa din Mund.</l>
            </lg>
            <trailer xml:id="idm139850575621984">
               <date xml:id="idm139850575621856">18/10 1914</date>.</trailer>
         </div>
      </body>
