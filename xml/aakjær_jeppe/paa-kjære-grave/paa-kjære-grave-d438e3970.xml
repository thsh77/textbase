         <div xml:id="workid61420" decls="#biblid61420">
            <head xml:id="idm139850577365168">
               <hi rend="italics" xml:id="idm139850577365040"><!--<emph>-->PAA KJÆRE GRAVE<!--</emph--></hi>.</head>
            <lg xml:id="idm139850577363856">
               <head xml:id="idm139850577363600">I.</head>
               <l xml:id="idm139850577363168">SAA mangen Slægt lig knækket Rør</l>
               <l xml:id="idm139850577362704">sank ned for denne Kirkedør.</l>
               <l xml:id="idm139850577362240">Velsignet I, som segned her</l>
               <l xml:id="idm139850577361808">i Læ for Sorgers Bygevejr.</l>
            </lg>
            <pb n="31" xml:id="s31" facs="adl/aakjaer/aakjaer2/aakj2031"/>
            <lg xml:id="idm139850577360064">
               <head xml:id="idm139850577359808">II.</head>
               <l xml:id="idm139850577359424">I ærlig Kamp, i ydmyg Tro</l>
               <l xml:id="idm139850577358976">I stille stred for Børn og Bo.</l>
               <l xml:id="idm139850577358512">Slog Kornet fejl paa karrig Jord,</l>
               <l xml:id="idm139850577358080">I rejstes ved et Fadervor.</l>
            </lg>
            <trailer xml:id="idm139850577357520">
               <date xml:id="idm139850577357392">Januar 1910</date>.</trailer>
         </div>
