   <text>
      <body>
         <div xml:id="workid74990" decls="#biblid74990">
            <head xml:id="idm139850575667168">
               <hi rend="italics" xml:id="idm139850575667040"><!--<emph>-->SØLVBRYLLUP<!--</emph--></hi>.</head>
            <lg xml:id="idm139850575665856">
               <l xml:id="idm139850575665600">FEMOGTYV' Aar</l>
               <l xml:id="idm139850575665216">de gi'r mange Slags Kaar,</l>
               <l xml:id="idm139850575664784">de gi'r Jubel og Sorg, alt som Huen den staar.</l>
               <l xml:id="idm139850575664336">Femogtyv' Aar</l>
               <l xml:id="idm139850575663920">gi'r den Smule, vi naar,</l>
               <l xml:id="idm139850575663488">giver Godtvejr og graat Vejr og Høst efter Vaar.</l>
            </lg>
            <pb n="108" xml:id="s108" facs="adl/aakjaer/aakjaer2/aakj2108"/>
            <lg xml:id="idm139850575645616">
               <l xml:id="idm139850575645360">Femogtyv' Aar</l>
               <l xml:id="idm139850575644944">de gi'r Føl, de gi'r Faar,</l>
               <l xml:id="idm139850575644496">de gi'r tusinde Ting., som en Bonde attraar.</l>
               <l xml:id="idm139850575644048">Femogtyv' Aar</l>
               <l xml:id="idm139850575643632">gi'r en Mage og Maar,</l>
               <l xml:id="idm139850575643216">giver Børn i din Stue og Liv i din Gaard.</l>
            </lg>
            <lg xml:id="idm139850575642608">
               <l xml:id="idm139850575642352">Femogtyv' Aar</l>
               <l xml:id="idm139850575641936">giver Sølv i dit Haar,</l>
               <l xml:id="idm139850575641488">ogsaa Sølv i din Pung, om dig Lykken bistaar.</l>
               <l xml:id="idm139850575640992">Femogtyv' Aar</l>
               <l xml:id="idm139850575640576">giver Skrammer og Skaar,</l>
               <l xml:id="idm139850575640144">giver let Sind og træt Sind, mens Skyerne gaar.</l>
            </lg>
            <lg xml:id="idm139850575639520">
               <l xml:id="idm139850575639264">Femogtyv' Aar</l>
               <l xml:id="idm139850575638848">gi'r den Lykke, du faar,</l>
               <l xml:id="idm139850575638416">gi'r dig Pligtslid og Gigtslid i Lænder og Laar.</l>
               <l xml:id="idm139850575637920">Femogtyv' Aar</l>
               <l xml:id="idm139850575637504">gi'r en Mand i hans Gaard,</l>
               <l xml:id="idm139850575637072">gi'r dig Præg, som du bærer, til Domsklokken slaar.</l>
            </lg>
            <lg xml:id="idm139850575636432">
               <l xml:id="idm139850575636176">Femogtyv' Aar!</l>
               <l xml:id="idm139850575635760">Om I sleger hæ' slaar,</l>
               <l xml:id="idm139850575635312">kom I bare <hi rend="italics" xml:id="idm139850575635024">igjen</hi>, om med Hæder I gaar.</l>
               <l xml:id="idm139850575634144">Femogtyv' Aar!</l>
               <l xml:id="idm139850575633728">Kom med Sødme som Saar,</l>
               <l xml:id="idm139850575633280">vi skal vandre jer ud, om vi stumler hæ' staar!</l>
            </lg>
            <trailer xml:id="idm139850575632656">
               <date xml:id="idm139850575632528">9/10 1914</date>.</trailer>
         </div>
