         <div xml:id="workid68460" decls="#biblid68460">
            <head xml:id="idm140299312595184">
               <hi rend="italics" xml:id="idm140299312595056">Symbolum Joh. Christ. Sthenij, Roschild</hi>.<lb xml:id="idm140299312594352"/>
               <hi rend="italics" xml:id="idm140299312594224">
                  <emph xml:id="idm140299312593824">CONSTANS AMOR ET LABOR</emph>
               </hi>
               <lb xml:id="idm140299312593536"/>
               <hi rend="italics" xml:id="idm140299312593408">
                  <emph xml:id="idm140299312593008">OMNIA VINCIT.</emph>
               </hi>
               <lb xml:id="idm140299312592720"/>
            </head>
            <p xml:id="idm140299312592464">
               <hi rend="italics" xml:id="idm140299312592336">Item</hi>.</p>
            <p xml:id="idm140299312591520">
               <hi rend="italics" xml:id="idm140299312591392">Sponsa decens virtus: Sponsus labor, ambit hic illa</hi>m,<lb xml:id="idm140299312590624"/>
               <hi rend="italics" xml:id="idm140299312590496">Fit conjunx: Novus hinc gloria fætus adest</hi>.</p>
            <p xml:id="idm140299312589616">Det er:</p>
            <lg xml:id="idm140299312589200">
               <l xml:id="idm140299312588944">
                  <hi rend="italics" xml:id="idm140299312588816">Virtus</hi> er en Brud saa skon,</l>
               <l xml:id="idm140299312587968">
                  <hi rend="italics" xml:id="idm140299312587840">Labor</hi> en Brudgam meget køn,</l>
               <l xml:id="idm140299312586960">Hand frjer til hende, hun siger hannem Ia,</l>
               <l xml:id="idm140299312586512">De affler en Daatter heder <hi rend="italics" xml:id="idm140299312586208">Gloria</hi>.</l>
            </lg>
            <trailer xml:id="idm140299312585264">
               <hi rend="italics" xml:id="idm140299312585136">
                  <emph xml:id="idm140299312584736">FINIS</emph>
               </hi>.</trailer>
         </div>
      </body>
