         <div xml:id="workid67440" decls="#biblid67440">
            <head xml:id="idm140562722469744">LANDSKAB [I].</head>
            <lg xml:id="idm140562722469328">
               <l xml:id="idm140562722469072">Stille, du elskede Kvinde!</l>
               <l xml:id="idm140562722468640">Tyst maa vi træde, vi to.</l>
               <l xml:id="idm140562722468192">Der sover en Sang her inde</l>
               <l xml:id="idm140562722467760">I Skovens natlige Ro.</l>
            </lg>
            <lg xml:id="idm140562722467216">
               <l xml:id="idm140562722466960">Stille er Vover og Vinde,</l>
               <l xml:id="idm140562722466528">Tavs er hver Sangfuglemund,</l>
               <l xml:id="idm140562722466096">Tiende Kilderne rinde</l>
               <l xml:id="idm140562722465680">Blankt over mossede Bund.</l>
            </lg>
            <lg xml:id="idm140562722465120">
               <l xml:id="idm140562722464864">Maanestraalerne spille</l>
               <l xml:id="idm140562722464448">Tyst mellem Bøgene frem,</l>
               <l xml:id="idm140562722464000">Langs ad Stierne stille</l>
               <l xml:id="idm140562722463584">Blunder en lyslig Bræm.</l>
            </lg>
            <lg xml:id="idm140562722463008">
               <l xml:id="idm140562722462752">Sølvskyen selv der oppe</l>
               <l xml:id="idm140562722462304">Hviler paa Vingen bred,</l>
               <l xml:id="idm140562722461888">Højt over Træernes Toppe</l>
               <l xml:id="idm140562722461440">Skuer den lyttende ned.</l>
            </lg>
            <lg xml:id="idm140562722460896">
               <l xml:id="idm140562722460640">Stille er Vover og Vinde,</l>
               <l xml:id="idm140562722460208">Tyst maa vi træde, vi to.</l>
               <l xml:id="idm140562722459760">Der sover en Sang her inde</l>
               <l xml:id="idm140562722459328">I Skovens natlige Ro.</l>
            </lg>
         </div>
