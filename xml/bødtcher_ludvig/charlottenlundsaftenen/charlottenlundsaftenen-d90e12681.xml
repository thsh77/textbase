         <div xml:id="workid80709" decls="#biblid80709">
            <head xml:id="idm140312757163888">CHARLOTTENLUNDSAFTENEN</head>
            <lg xml:id="idm140312757163472">
               <l xml:id="idm140312757163216">Aftenvinde</l>
               <l xml:id="idm140312757162800">Svæve, svinde</l>
               <l xml:id="idm140312757162368">Gjennem Skovens Sommersal:</l>
               <l xml:id="idm140312757161936">Blomsterduften</l>
               <l xml:id="idm140312757161520">Krydrer Luften,</l>
               <l xml:id="idm140312757161104">Nys faldt Regnen mild og sval:</l>
               <l xml:id="idm140312757160672">Dybt bag Grene</l>
               <l xml:id="idm140312757160256">Ømt og ene</l>
               <l xml:id="idm140312757159840">Sværmer sødt en Nattergal: -</l>
            </lg>
            <lg xml:id="idm140312757159248">
               <l xml:id="idm140312757158992">Aftenvinde</l>
               <l xml:id="idm140312757158576">Svæve, svinde</l>
               <l xml:id="idm140312757158144">Hviske i den dunkle Sal: -</l>
               <l xml:id="idm140312757157712">O kom o kom min Sjels Veninde!</l>
               <l xml:id="idm140312757157280">Dig venter Nattergalens Slag!</l>
               <l xml:id="idm140312757156848">Dig venter under Skovens Tag</l>
               <l xml:id="idm140312757156416">Mit eget Hjertes varme Slag!</l>
            </lg>
            <pb n="243" xml:id="s243" facs="adl/boedtcher/boedtcher1/boed1243"/>
         </div>
